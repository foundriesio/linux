	/* CP */
	auth-cp {
		compatible = "telechips, tcc-cp";
		cp-type = <1>;	    /* 2.0C = 1, 2.0B = 0 */
		i2c-channel = <2>;	/* 0 ~ 3 */
		/*	power-ctrl-able; */
		/* cp_power-gpio = <&gpext2 14 0>; */
		cp_reset-gpio = <&gpg 19 0>;
	};

	i2s@76201000 {
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&mdai_clks &mdai_d0>;
		pinctrl-1 = <&mute_mdai_d0>;
		status = "okay";
	};

	i2s@76101000 {
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&s3dai_clks &s3dai_d0>;
		pinctrl-1 = <&mute_s3dai_d0>;
		status = "okay";
	};

	i2s@76401000 {
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&s1dai_clks &s1dai_d0>;
		pinctrl-1 = <&mute_s1dai_d0>;
		status = "okay";
	};
/*
	i2s@76b01000 {
		pinctrl-names = "default";
		pinctrl-0 = <&s2dai_clks &s2dai_d0>;
		status = "okay";
	};
*/
	spdif@76202000 {
		pinctrl-names = "default";
		pinctrl-0 = <&mdai_spdif_tx &mdai_spdif_rx>;
		status = "okay";
	};

	spdif@76102000 {
		pinctrl-names = "default";
		pinctrl-0 = <&s3dai_spdif_tx>;
		status = "okay";
	};

	dummy: dummy@0 {
		compatible = "telechips,dummy";
	};

	sound {
		compatible = "telechips,snd-lcn3";
		telechips,model = "TCC Audio Card";

		telechips,dai-controller = <&i2s1 &spdif0 &i2s0 &spdif1>;
		telechips,audio-codec = <&ak4601 &dummy &dummy &dummy>;

		status="okay";
	};

	videosource {
		compatible = "telechips,videosource";
		status = "okay";

		pinctrl-names = "idle", "active";
		pinctrl-0 = <&cam4_idle>;
		pinctrl-1 = <&cam4_clk &cam4_data>;

		atv_adv7182 {
			rst-gpios = <&gpe  4 1>;
		};
	};

	switch_reverse {
		compatible = "telechips,switch_reverse";
		status = "okay";

		pinctrl-names = "default";
		pinctrl-0 = <&switch_reverse2>;

		switch-gpios = <&gpd 4 0>;
		switch-active = <1>;
	};

	overlaymixer:overlaymixer@72400000 {
		//status ="okay";
		contiguousSize = <0x1000000>;  /* 16MB (if > 512MB RAM) refer to Guide 27 Page */
		baseAddress = <0x80000000>;
		physSize = <0x80000000>; /*  DRAM size (0x40000000 = 1GB, 0x80000000 = 2GB) */
	};

	fbdisplay: fbdisplay@72000000 {
		telechips,fbdisplay_num = <0>;
		status ="okay";
	};

	tcc_vsync_drv {
		compatible = "telechips,tcc_vsync";
		display-vioc0 = <&vioc_disp VIOC_DISP0>;
		display-vioc1 = <&vioc_disp VIOC_DISP1>;
		status = "okay";
	};

	tcc_video_viqe {
		status ="okay";

		tcc_video_viqe_lcd {
			telechips,sc = <&vioc_scaler VIOC_SCALER3>;
			//scaler_num_60 = <3>;
		};
	};

	vioc_lut:vioc_lut@72009000{
		status = "okay";
	};

	wdma_drv0:wdma_drv@0 {
		status = "okay";
		scalers = <&vioc_scaler VIOC_SCALER2>, <&vioc_scaler VIOC_SCALER2>;
	};

	hdmi@72300000 {
		status = "okay";
		vioc_lcdc_num = <0>;
		no_gpio_manual_control = <1>;
	};

	hpd@72310000 {
		status = "okay";
	};

	audio@72330000 {
		status = "okay";
	};

	cec@72370000 {
		status = "okay";
	};

	gmac@71600000 {
		status = "okay";
		phyrst-gpio = <&gpc 29 0>;
		phyon-gpio = <&gpc 19 0>;
		phy-interface = "rgmii";
		ecid-mac-addr;
		txclk-o-dly = <0>; // changed from <29>;
		txclk-o-inv = <0>;
		txclk-i-dly = <0>;
		txclk-i-inv = <0>;
		txen-dly = <0>;
		txer-dly = <0>;
		txd0-dly = <0>;
		txd1-dly = <0>;
		txd2-dly = <0>; // <3>;
		txd3-dly = <0>; // <3>;
		txd4-dly = <0>;
		txd5-dly = <0>;
		txd6-dly = <0>;
		txd7-dly = <0>;
		rxclk-i-dly = <0>;
		rxclk-i-inv = <0>;
		rxdv-dly = <0>; // changed from <15>;
		rxer-dly = <0>;
		rxd0-dly = <0>; //<22>;
		rxd1-dly = <0>; //<20>;
		rxd2-dly = <0>; //<20>;
		rxd3-dly = <0>; //<21>;
		rxd4-dly = <0>;
		rxd5-dly = <0>;
		rxd6-dly = <0>;
		rxd7-dly = <0>;
		crs-dly = <0>;
		col-dly = <0>;
		pinctrl-names = "default", "mii", "rmii", "gmii", "rgmii";
		pinctrl-0 = <>;
		pinctrl-1 = <&gmac1_mdc &gmac1_mdio &gmac1_col &gmac1_crs &gmac1_txer &gmac1_rxer
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3>;
		pinctrl-2 = <&gmac1_mdc &gmac1_mdio
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1
					 &gmac1_rxer &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1>;
		pinctrl-3 = <&gmac1_mdc &gmac1_mdio &gmac1_col &gmac1_crs &gmac1_txer &gmac1_rxer
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_txd4 &gmac1_txd5 &gmac1_txd6 &gmac1_txd7
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3
					 &gmac1_rxd4 &gmac1_rxd5 &gmac1_rxd6 &gmac1_rxd7>;
		pinctrl-4 = <&gmac1_mdc &gmac1_mdio
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3>;
	};

	lvds: lvds@72380040{
		status = "okay";
	};

	fld0800 {
		compatible = "telechips,lvds-fld0800";
		status = "okay";
		lvds0 = <&lvds>;
		//power-on-gpios	= ;			//LCD_ON
		display-on-gpios	= <&gpd 6 0>;		//LCD_DISP
		reset-gpios		= <&gpf 19 0>;		//LCD_RST
		lvds-stby-gpios		= <&gpb 19 0>;		//LCD_DE = LVDS_STBY in LVDS
		//lvds-power-gpios	= ;			//LVDS_EN
		//lvds-power-gpios1	= ;			//V_5V0_EN
	};

	panel_1920x720 {
		status = "disable";
		compatible = "telechips,hdmi1920x720";
		clocks = <&clk_peri PERI_LCD0>, <&clk_peri PERI_LCD1>, <&clk_peri PERI_HDMI>, <&clk_ddi DDIBUS_HDMI>, <&clk_isoip_top ISOIP_TOP_HDMI>;
		clock-names = "lcdc0-clk", "lcdc1-clk", "hdmi-pclk", "hdmi-hclk", "hdmi-phy";
	};

	backlight {
		compatible = "telechips,tco_backlight";
		tco = <1 50>;	/* unit:nsec. */
		bl-gpios = <&gpc 19 4>;
		bl-gpio-func = <4>;
		default-brightness = <102>;	/* MAX:255 */
		status = "okay";
		pinctrl-names = "active", "idle";
		pinctrl-0 = <&tco_0_0>;
		pinctrl-1 = <&tco_0_0_idle>;
	};

	thermal@74700000 {
		compatible = "telechips,tcc-thermal";
		reg = <0x74700000 0x4 0x74700030 0x4 0x74200290 0x4 0x7420029C 0x4>;

		throttle_count = <5>;
		throttle_active_count = <0>;
		throttle_passive_count = <5>;

		cal_type = "TYPE_ONE_POINT_TRIMMING";
		polling-delay-idle = <0>;
		polling-delay-passive = <0>;

		throttle_table {
			throttle_tab_0 {
				temp = <75>;
				freq_max_cluster0 = <1000000>;
				freq_max_cluster1 = <900000>;
			};
			throttle_tab_1 {
				temp = <80>;
				freq_max_cluster0 = <900000>;
				freq_max_cluster1 = <810000>;
			};
			throttle_tab_2 {
				temp = <85>;
				freq_max_cluster0 = <800000>;
				freq_max_cluster1 = <720000>;
			};
			throttle_tab_3 {
				temp = <90>;
				freq_max_cluster0 = <700000>;
				freq_max_cluster1 = <630000>;
			};
			throttle_tab_4 {
				temp = <95>;
				freq_max_cluster0 = <600000>;
				freq_max_cluster1 = <540000>;
			};
		};
	};

	cpu-id@74200290{
		compatible = "telechips,tcc-cpu-id";
		reg = <0x74200290 0x4 0x74200298 0x4 0x7420029C 0x4 0x74400014 0x4>;
		status = "okay";
	};

	adc@76064000 {
		status = "okay";
	};

	tcckey@76064000 {
		compatible = "telechips,tcckey";
		reg = <0x76064000 0x24>;
		status = "okay";
	};
};

/* console */
&uart0 {
        pinctrl-names = "default";
        pinctrl-0 = <&uart22_data>;
        port-mux = <22>;
        status = "okay";
};

/* bluetooth */
&uart1 {
        pinctrl-names = "default";
        pinctrl-0 = <&uart30_data &uart30_rtscts>;
        port-mux = <30>;
        dmas = <&dma0 0 29 &dma0 1 30>;
        dma-names = "tx", "rx";
        status = "okay";
};

/* GPS */
&uart3 {
        pinctrl-names = "default";
        pinctrl-0 = <&uart10_data>;
        port-mux = <10>;
        status = "okay";
};


/{
	firmware {
		android {
			compatible = "android,firmware";
			fstab {
				compatible = "android,fstab";
				vendor {
					compatible = "android,vendor";
					dev = "/dev/block/platform/76020600.sdhc/by-name/vendor";
					type = "ext4";
					mnt_flags = "ro, wait";
					fsmgr_flags = "wait";
				};
			};
		};
	};
};

&reboot_mode {
	mode-shell = <BOOT_NORMAL>;
	mode-bootloader = <BOOT_FASTBOOT>;
	mode-recovery = <BOOT_RECOVERY>;
};
