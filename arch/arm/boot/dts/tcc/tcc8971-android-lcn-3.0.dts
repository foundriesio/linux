/dts-v1/;

#include "tcc8971.dtsi"

/ {
	model = "Telechips TCC8971 LCN Board SV3.0";
	compatible = "telechips,tcc8971-lcn", "telechips,tcc897x";

	ckc@74000000 {
		adjust-audio-clk;	/* adjust audio clock for A2DP plaing */
	};

	tcc_bluetooth {
		compatible = "telechips, tcc_bluetooth";
		bt_power-gpio = <&gpc 21 0>;
		bt_reg_on-gpio = <&gpf 31 0>;
		status = "okay";
	};

	tcc_bt_dev {
		compatible = "telechips, tcc_bt_dev";
		status = "okay";
	};

	gps-gpio {
		compatible = "telechips, gps-gpio";
		gps_power-gpios = <&gpc 25 0>;
	};

	i2c@76300000 {
		status = "okay";
		port-mux = <11>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c11_bus>;

		ilitek@41 {
			compatible = "ilitek,aimvF";
			reg = <0x41>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		gt813@5d {
			compatible = "goodix,gt813";
			reg = <0x5d>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		vtl_ct26x@01 {
			compatible = "vtl,ct36x";
			reg = <0x01>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		ssd253x@48 {
			compatible = "solomon,ssd253x";
			reg = <0x48>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};
	};

	i2c@76310000 {
		status = "okay";
		port-mux = <18>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c18_bus>;

		videosource@adv7182 {
			compatible = "videosource,adv7182";
			reg = <0x21>;	// 0x42 >> 1
		};

		videosource@dm5886 {
			compatible = "videosource,dm5886";
			reg = <0x60>;	// 0xC0 >> 1
		};
	};

	i2c@76320000 {
		status = "okay";
		port-mux = <23>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c23_bus>;
	};

	i2c@76330000 {
		status = "okay";
		port-mux = <12>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c12_bus>;

		ak4601: ak4601@10 {
			compatible = "akm,ak4601";
			//ak4601,pdn-gpio = <&gpe 4 1>;
			reg = <0x10>;
		};

		pmic: rt5028@3f {
			compatible = "Richtek,rt5028";
			reg = <0x3f>;
			int-gpios = <&gpe 31 0>;

			regulators {
				RT5028_ID_BUCK1: buck1 {
					regulator-name = "vdd_core";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK2: buck2 {
					regulator-name = "vdd_mem";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK3: buck3 {
					regulator-name = "vdd_cpu";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK4: buck4 {
					regulator-name = "vdd_io3v3";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO1: ldo1 {
					regulator-name = "vd2_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO2: ldo2 {
					regulator-name = "vdd_io1v8";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO3: ldo3 {
					regulator-name = "vdd_btwifi";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO4: ldo4 {
					regulator-name = "vdd_vbat";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO5: ldo5 {
					regulator-name = "vdd_gps";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO6: ldo6 {
					regulator-name = "vd1_io";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO7: ldo7 {
					regulator-name = "vd1_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO8: ldo8 {
					regulator-name = "vd2_io";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
			};
		};
	};

	smui2c@74500000 {
		status = "okay";
	};

	gpsb0@76900000 {
		status = "okay";
		gpsb-port = <12>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb12_bus_idle>;
		pinctrl-1 = <&gpsb12_bus_spi>;

		#address-cells = <1>;
		#size-cells = <0>;

		spidev@0 {
			compatible = "rohm,dh2228fv";
			reg = <0>;
			spi-max-frequency = <2000000>;
		};
	};

	gpsb1@76910000 {
		status = "okay";
		gpsb-port = <13>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb13_bus_idle>;
		pinctrl-1 = <&gpsb13_bus_tsif>;
	};

	gpsb2@76920000 {
		status = "okay";
		gpsb-port = <12>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb12_bus_idle>;
		pinctrl-1 = <&gpsb12_bus_tsif>;
	};

	ehci_phy@71EA0010{
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
	};

	ehci@71200000 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
	};

	ohci@71300000 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
	};

	dwc_otg_phy@71EA0100 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 3 0>;
	};

	dwc2@71B00000 {
		status = "okay";
		phy-type = <1>;		//pci phy = 1, nano phy = 0
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 3 0>;
	};

	dwc_otg@71B00000 {
		status = "okay";
		phy-type = <1>;     //pico phy = 1, nano phy = 0
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 3 0>;
	};

	/* SD CARD */
	vqmmc_sd: vqmmc_sd {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_sd";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	sdhc@76020000 {
		status = "okay";
#ifndef CONFIG_MMC_SDHCI_TCC_MODULE_ONLY
		compatible = "telechips,tcc897x-sdhci";
#else
		/*
		 * In order to reduce boot time,
		 * we use the driver as module not built-in.
		 */
		compatible = "telechips,tcc897x-sdhci,module-only";
#endif
		controller-id = <0>;
		bus-width = <4>;
		max-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd5_clk &sd5_cmd &sd5_bus4>;

		broken-cd;
		cd-inverted;
		keep-power-in-suspend;

		wp-gpios = <&gpf 20 0>;
		cd-gpios = <&gpf 21 0>;

		vqmmc-supply = <&vqmmc_sd>;
	};
#if 1 // 0 : force detect, 1 : not force detect
	sdhc@76020400 { // Wi-Fi tcm3840
		status = "okay";
		controller-id = <2>;
		max-frequency = <208000000>;
		//cd-gpios = <&gpb 13 0>;
		//wp-gpios = <&gpext1 3 0>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd0_clk_idle &sd0_cmd_idle &sd0_bus4_idle &out0_clk>;
