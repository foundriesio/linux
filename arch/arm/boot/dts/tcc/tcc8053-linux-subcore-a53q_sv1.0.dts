	mipi_csi2_0: mipi_csi2_0@1bc00800 {
		compatible = "telechips,tcc805x-mipi-csi2";
		status = "okay";
		reg = 	<0x1bc00800 0x120>,	/* csi0_base */
		      	<0x1bc00a00 0x05c>,	/* gdb0_base */
		      	<0x1bc00000 0x014>,	/* ckc_base */
			<0x1bc00400 0x120>;	/* cfg_base */
		reg-names = "csi", "gdb", "ckc", "cfg";

		clock-frequency = <600000000>;

		interrupts =	<GIC_SPI 158 IRQ_TYPE_LEVEL_HIGH>,// MIPI0 CSIS
				<GIC_SPI 159 IRQ_TYPE_LEVEL_HIGH>;// MIPI0 Generic
		/*
		 * 0: select output of MIPI0
		 * 1: select output of MIPI1
		 */
		mipi-chmux-0 = <0>;
		mipi-chmux-1 = <0>;
		mipi-chmux-2 = <0>;
		mipi-chmux-3 = <0>;

		/*
		 * 0: select output of MIPI1
		 * 1: select output of MIPI0
		 */
		mipi-chmux-4 = <0>;
		mipi-chmux-5 = <0>;
		mipi-chmux-6 = <0>;
		mipi-chmux-7 = <0>;

		/*
		 * 1: bypass isp
		 * 0: use isp
		 */
		isp0-bypass = <1>;
		isp1-bypass = <1>;
		isp2-bypass = <1>;
		isp3-bypass = <1>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			/*
			 * 0
			 * input port.
			 *
			 * 1 ~ 4
			 * output ports. (1: VC0 ~ 4: VC3)
			 */
			port@0 {
				reg = <0>;
				mipi_csi2_0_in: endpoint {
#if !defined(USE_FHD_VIDEOSOURCES)
					remote-endpoint	= <&max9286_out>;
#else
					remote-endpoint	= <&max96712_out>;
#endif//!defined(USE_FHD_VIDEOSOURCES)
					io-direction	= "input";
					num-channel = <4>;

					/*
					 * 0: CH0 only, no data interleave
					 * 1: DT only
					 * 2: VC only
					 * 3: VC and DT
					 */
					interleave-mode = <3>;
					//hs-settle = <2>; // for mcnex ispless 1ch
					//hs-settle = <19>; // for sekonix ispless 4ch
#if !defined(USE_FHD_VIDEOSOURCES)
					hs-settle = <17>; // for HD 4ch
#else
					hs-settle = <37>; // for FHD 4ch
#endif//!defined(USE_FHD_VIDEOSOURCES)
					data-lanes = <1 2 3 4>;
				};
			};
			port@1 {
				reg = <1>;
				mipi_csi2_0_out0: endpoint {
#if defined(USE_TELECHIPS_ISP)
					remote-endpoint = <&isp0_in>;
#else
					remote-endpoint	= <&videoinput4_in>;
#endif//defined(USE_TELECHIPS_ISP)
					io-direction	= "output";
					channel		= <0>;
					/*
					 * 0: Single pixel mode
					 * 1: Dual pixel mode (RAW8/10/12, YUV422)
					 * 2: Quad pixel mode (RAW8/10/12)
					 * 3: Invalid
					 */
					pixel-mode = <1>;
				};
			};
			port@2 {
				reg = <2>;
				mipi_csi2_0_out1: endpoint {
					remote-endpoint	= <&videoinput5_in>;
					io-direction	= "output";
					channel		= <1>;
					pixel-mode = <1>;
				};
			};
			port@3 {
				reg = <3>;
				mipi_csi2_0_out2: endpoint {
					remote-endpoint	= <&videoinput6_in>;
					io-direction	= "output";
					channel		= <2>;
					pixel-mode = <1>;
				};
			};
			port@4 {
				reg = <4>;
				mipi_csi2_0_out3: endpoint {
					remote-endpoint	= <&videoinput7_in>;
