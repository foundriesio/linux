&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart9_data>;
	status = "okay";
};

&i2c7 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	port-mux = <12>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c12_bus>;

	/* video decoder using parallel interface output */
	adv7182: adv7182@21 {
		status		= "okay";
		compatible	= "adi,adv7182";
		pinctrl-names	= "default";
		pinctrl-0	= <&cam0_clk &cam0_data_8bit>;
		rst-gpios	= <&gpb 14 0>;
		reg		= <0x21>;	// 0x42 >> 1
		port {
			adv7182_out: endpoint {
				remote-endpoint = <&videoinput0_in>;
				io-direction	= "output";
				bus-width	= <8>;
			};
		};
	};

#if !defined(USE_FHD_VIDEOSOURCES)
#if defined(USE_ARXXXX)
	arxxxx: arxxxx@50 {
		status		= "okay";
		compatible	= "onnn,arxxxx";
		reg		= <0x50>;
		port {
			arxxxx_out: endpoint {
				remote-endpoint = <&max96701_in>;
				io-direction	= "output";
			};
		};
	};
#elif defined(USE_AR0147)
	/*
	 * ISPless camera module.
	 * mcnex(for ecarx demo) camera module: max96701, max9286
	 * sekonix(for mobis svm) camera module: max96701, max96712
	 */
	ar0147: ar0147@10 {
		/* Camera Image Sensor */
		status		= "okay";
		compatible	= "onnn,ar0147";
		reg		= <0x10>;
		port {
			ar0147_out: endpoint {
				remote-endpoint = <&max96701_in>;
				io-direction	= "output";
			};
		};
	};
#endif

	/* HD camera module & max9286 using MIPI CSI2 output */
	max96701: max96701@40 {
		/* serializer */
		status		= "okay";
		compatible	= "maxim,max96701";
		reg		= <0x40>;
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			port@0 {
				reg = <0>;
				max96701_in: endpoint {
#if defined(USE_ARXXXX)
					remote-endpoint = <&arxxxx_out>;
#elif defined(USE_AR0147)
					remote-endpoint = <&ar0147_out>;
#endif
					io-direction	= "input";
				};
			};
			port@1 {
				reg = <1>;
				max96701_out: endpoint {
					remote-endpoint = <&max9286_in0>;
					io-direction	= "output";
				};
			};
		};
	};

	max9286: max9286@48 {
		/* deserializer */
		status		= "okay";
		compatible	= "maxim,max9286";
		pwd-gpios	= <&gpg 5 1>;
		intb-gpios	= <&gph 11 1>;
		//rst-gpios	= <&gpb 15 1>;  /* mcnex module sensor */
		reg		= <0x48>;	// 0x90 >> 1
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			/*
			 * 0 ~ 3
			 * input ports. The number is matched with VC
			 *
			 * 4
			 * output port.
			 */
			port@0 {
				reg = <0>;
				max9286_in0: endpoint {
					remote-endpoint = <&max96701_out>;
					io-direction	= "input";
					channel		= <0>;
				};
			};
			port@1 {
				reg = <1>;
				max9286_in1: endpoint {
					remote-endpoint = <&max96701_out>;
					io-direction	= "input";
					channel		= <1>;
				};
			};
			port@2 {
				reg = <2>;
				max9286_in2: endpoint {
					remote-endpoint = <&max96701_out>;
					io-direction	= "input";
					channel		= <2>;
				};
			};
			port@3 {
				reg = <3>;
				max9286_in3: endpoint {
					remote-endpoint = <&max96701_out>;
					io-direction	= "input";
					channel		= <3>;
				};
			};
			port@4 {
				reg = <4>;
				max9286_out: endpoint {
					remote-endpoint = <&mipi_csi2_0_in>;
					io-direction	= "output";
				};
			};
		};
	};
#else

	/* FHD camera module & max96712 using MIPI CSI2 output */
	cxd5700: cxd5700@18 {
		/* ISP of camera module */
		status		= "okay";
		compatible	= "sony,cxd5700";
		reg		= <0x18>;
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
