	sdhc@16460000 { // Wi-Fi tcm3800
		status = "okay";
		controller-id = <2>;
		bus-width = <4>;
		max-frequency = <200000000>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd2_gpsd0_bus4_idle>;
		pinctrl-1 = <&sd2_gpsd0_clk &sd2_gpsd0_cmd &sd2_gpsd0_bus4 &out5_clk>;
		clocks = <&clk_peri PERI_SDMMC2 &clk_io IOBUS_SDMMC2 &clk_peri PERI_OUT5>;
		clock-names = "mmc_fclk", "mmc_hclk", "ext32";

		cd-inverted;
		//wp-inverted;
		//non-removable;
		//broken-cd;
		keep-power-in-suspend;
		//enable-sdio-wakeup;
		//cap-power-off-card;
		cap-sdio-irq;
		cap2-no-sleep-cmd;
		cap-bus-width-test;

		//wp-gpios = <&gpb 7 0>;
		//cd-gpios = <&gpext1 5 0>;
		//vctrl-gpio = <&gpsd 10 0>;
		//pwr-gpio = <&gpext2 7 0>;

		wifi-polling;
		ext32-enable; // make_rtc
		cap-adma;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;
	};

	/* bluetooth */
	tcc_bt_dev {
		compatible = "telechips, tcc_bt_dev";
		bt_reset-gpio = <&gpk 26 0>;
		status = "okay";
	};
	
	tcm3800 {
		compatible = "telechips, tcm3800";
		wlreg_on-gpio = <&gpsd0 7 0>;
	};
	
	tcckey@16050000 {
	    compatible = "telechips,tcckey";
	    status = "okay";
	};

	vout_drv@0 {
		status = "okay";
	};
	smartcard {
		compatible = "telechips,pl131-smartcard";
		reg = <0x16601000 0xFFF 0x16684000 0x10>;                //smartcard #0, 
		clocks = <&clk_io IOBUS_SMARTCARD0>;
		clock-frequency = <50000000>;
		smartcard-clock = <4000000>;                             // initail value 4Mhz
		Fi = <372>;						 // clock rate conversion integer
		Di = <1>;						 // initail value

		uart_ch = <&uart1>;					 // uart and smartcard share clock
		uart_chnum = <1>;
		port-num = <0>;
		tda8024-use = <1>;
		rst_gpios =  <&gpb 16 0>;
		detect_gpios = <&gph 7 0>;
		pwren_gpios = <&gph 10 0>;
		data_gfb = <21>;
		clk_gfb = <22>;
		pinctrl-names = "default";
		pinctrl-0 = <&smartcard_bus>;
		status = "disable";
	};
};
