&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart20_data &uart20_rtscts>;
	dmas = <&udma1 2 2 &udma1 3 2>;
	dma-names = "tx", "rx";
	status = "okay";
};

/* H/W Demux */
&hwdemux {
	compatible = "telechips,hwdemux";
	status = "okay";
};

&tcc_hwdmx_tsif0 {
	compatible = "telechips,tcc89xx-hwdmx-tsif";
	tsif-port = <1>;
	pinctrl-names = "idle", "active";
	pinctrl-0 = <&ts0_idle>;
	pinctrl-1 = <&ts0_active>;
	status = "okay";
};

&tcc_hwdmx_tsif1 {
	compatible = "telechips,tcc89xx-hwdmx-tsif";
	tsif-port = <2>;
	pinctrl-names = "idle", "active";
	pinctrl-0 = <&ts1_idle>;
	pinctrl-1 = <&ts1_active>;
};

/* ISDB-T */
&tcc_isdbt_ctrl {
	compatible = "telechips,tcc_isdbt_ctrl";
};

/* eMMC */
&sd0_clk {
		telechips,no-pull;
};

&sd0_cmd {
		telechips,no-pull;
};

&sd0_bus8 {
		telechips,no-pull;
};

/* SD slot */
&sd1_clk {
		telechips,no-pull;
};

&sd1_cmd {
		telechips,no-pull;
};

&sd1_bus4 {
		telechips,no-pull;
};
