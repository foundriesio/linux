/* USB 3.0 DRD */
&dwc3_phy {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpext1 2 0>;
};
&dwc3 {
	status = "okay";                   
	//usb3en-ctrl-able;                
	//usb3en-gpio = <&gpc 20 0>;       
	//vbus-source-ctrl;                
	vbus-ctrl-able;                    
	vbus-gpio = <&gpext1 2 0>;
	//pinctrl-names = "default";       
	//vdd_dwc-supply = <&vdd_v5p0_reg>;
}; 

/* USB 2.0 OTG */
&ehci_phy {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpext1 1 0>;
};

&ehci {
	status = "okay";
	hcd_id = <0>;
	phy-type = <1>;	    //pico phy = 1, nano phy = 0
	vbus-ctrl-able;
	vbus-gpio = <&gpext1 1 0>;

	//sq_level_ctl;
	TXVRT = <0xB>;
	TXRISET = <0x3>;
	TXAT = <0x1>;
};

&ohci {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpext1 1 0>;
};

&dwc_otg_phy {
	status = "okay";

	vbus-ctrl-able;
	vbus-gpio = <&gpext1 0 0>;
};

&mhst_phy {
	status = "okay";

	vbus-ctrl-able;
	vbus-gpio = <&gpext1 0 0>;
};

&dwc_otg {
	status = "okay";
	phy-type = <1>;     //pico phy = 1, nano phy = 0

	vbus-ctrl-able;
	vbus-gpio = <&gpext1 0 0>;
};
