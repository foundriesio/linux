&aliases {
	videosource0 = &videosource0;
	videosource1 = &videosource1;
	videosource2 = &videosource2;
};

&pcie {
	reset-gpio = <&gpc 30 0>;
	//status = "okay";
};

/* USB 2.0 Host */
&ehci_phy {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpa 28 0>; /* 0 : GPIO_ACTIVE_HIGH, 1 : GPIO_ACTIVE_LOW */
};

&ehci {
	status = "okay";
	hcd_id = <0>;
	phy-type = <1>;	    //pico phy = 1, nano phy = 0
	TXVRT = <0xB>;
	TXRISET = <0x3>;
	TXAT = <0x1>;
};

&ohci {
	status = "okay";
};

/* USB 2.0 DRD */
&dwc_otg_phy {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpa 27 0>; /* 0 : GPIO_ACTIVE_HIGH, 1 : GPIO_ACTIVE_LOW */
};

&mhst_phy {
	status = "okay";
};

&dwc_otg {
	status = "okay";
	phy-type = <1>;     //pico phy = 1, nano phy = 0
};

/* USB 3.0 DRD */
&dwc3_phy {
	status = "okay";
	vbus-ctrl-able;
	vbus-gpio = <&gpa 29 0>; /* 0 : GPIO_ACTIVE_HIGH, 1 : GPIO_ACTIVE_LOW */
};

&dwc3 {
	status = "okay";
};

