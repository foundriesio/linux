&switch_reverse {
	compatible = "telechips,switch_reverse";
	status = "disabled";

	pinctrl-names = "default";
	pinctrl-0 = <&switch_reverse_ma15>;

	switch-gpios = <&gpma 15 1>;
	switch-active = <0>;
};

&thermal {
	        compatible = "telechips,tcc-thermal";

                reg = <0x14700000 0x4>, //0:Enable register
                      <0x14700004 0x4>, //1:Control register
                      <0x1470000C 0x4>, //2:Probe select register
                      <0x14200290 0x4>, //3:ECID Control register
                      <0x1420029C 0x4>, //4:ECID Data register
                      <0x147000D0 0x4 0x147000D4 0x4 0x147000D8 0x4 0x147000DC 0x4 0x147000E0 0x4 0x147000E4 0x4>, //5-10:TSENSOR Data register
                      <0x14700024 0x4>, //11:Interrupt Eable register
                      <0x14700034 0x4>, //12:Interrupt Status register
                      <0x14700050 0x4 0x14700054 0x4>, // 13, 14:Threshold Data register(MAIN)
                      <0x14700058 0x4 0x1470005C 0x4>, // 15, 16:Threshold Data register(Probe0)
                      <0x14700060 0x4 0x14700064 0x4>, // 17, 18:Threshold Data register(Probe1)
                      <0x14700068 0x4 0x1470006C 0x4>, // 19, 20:Threshold Data register(Probe2)
                      <0x14700070 0x4 0x14700074 0x4>, // 21, 22:Threshold Data register(Probe3)
                      <0x14700078 0x4 0x1470007C 0x4>, // 23, 24:Threshold Data register(Probe4)
                      <0x14700044 0x4>, //25:Interrupt Clear register
                      <0x1470002C 0x4>; //26:Interrupt Mask register

                status = "okay";
                throttle_count = <5>;
                throttle_active_count = <0>;
                throttle_passive_count = <5>;
                interrupts = <GIC_SPI 40 0>;
                cal_type = "TYPE_TWO_POINT_TRIMMING";
                polling-delay-idle = <0>;
                polling-delay-passive = <0>;

                threshold_high_temp = <85>;
                threshold_low_temp = <25>;


	throttle_table {
		throttle_tab_0 {
			temp = <75>;
			freq_max_cluster0 = <1000000>;
			freq_max_cluster1 = <900000>;
		};
		throttle_tab_1 {
			temp = <80>;
			freq_max_cluster0 = <900000>;
			freq_max_cluster1 = <810000>;
		};
		throttle_tab_2 {
			temp = <85>;
			freq_max_cluster0 = <800000>;
			freq_max_cluster1 = <720000>;
		};
		throttle_tab_3 {
			temp = <90>;
			freq_max_cluster0 = <700000>;
			freq_max_cluster1 = <630000>;
		};
		throttle_tab_4 {
			temp = <95>;
			freq_max_cluster0 = <600000>;
			freq_max_cluster1 = <540000>;
		};
	};
};

&cpu_id {
	compatible = "telechips,tcc-cpu-id";
	reg = <0x14200290 0x4 0x14200298 0x4 0x1420029C 0x4 0x14400014 0x4>;
	status = "okay";
};

&overlaymixer {
	status ="okay";
	contiguousSize = <0x1000000>;  /* 16MB (if > 512MB RAM) refer to Guide 27 Page */
	baseAddress = <0x20000000>;
	physSize = <0x80000000>; /*  DRAM size (0x40000000 = 1GB, 0x80000000 = 2GB) */
};

