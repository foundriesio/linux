/dts-v1/;

#include "tcc8031-linux-avn.dtsi"

/ {
	/* Describe here after remove this statement if you have something to be override. */
	/* SD CARD */
	sdhc1_pwrseq: sdhc1_pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpsd1 8 GPIO_ACTIVE_LOW>;
	};

	sdhc@16450000 {
		broken-cd;
		cd-inverted;
		keep-power-in-suspend;

		wp-gpios = <&gpsd1 7 0>;
		cd-gpios = <&gpsd1 6 0>;

		mmc-pwrseq = <&sdhc1_pwrseq>;
		vqmmc-supply = <&vdd_ldo3>;
	};
	
	/* SDIO */
	vqmmc_sdio: vqmmc_sdio {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_sdio";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	sdhc@16460000 {
		//non-removable;
		keep-power-in-suspend;
		cap-sdio-irq;

		vqmmc-supply = <&vqmmc_sdio>;

		cd-force-presence-change;

		tcm38xx {
			compatible = "telechips,tcm38xx";
			wifi_pwr-gpio = <&gpsd2 6 0>; /* BT_WIFI_VBAT_EN */
			wlreg_on-gpio = <&gpsd2 8 0>; /* WIFI_REG_ON */
			//wifi_pwr_1p8v-gpio = <&gpsd1 6 0>;
			//wifi_pwr_3p3v-gpio = <&gpsd1 7 0>;
		};
	};

	timer@14300000 {
		/* Timer 0 is used for WIFI LPO */
		tcc-timer-reserved = <0>;
	};

	tm123xdhp90 {
		compatible = "telechips,lvds-tm123xdhp90";
		status = "okay";
		lvds0 = <&lvds_phy>;
		power-on-gpios = <&gpb 17 0>;		// LCD_ON
		reset-gpios = <&gpb 18 0>;		// LCD_RST
		/*
		display-on-gpios = ;		// LCD_DISP
		lvds-stby-gpios = ;		// LCD_DE = LVDS_STBY in LVDS
		lvds-power-gpios = ;		// LVDS_EN
		lvds-power-gpios1 = ;		// V_5V0_EN
		*/
		mode = <1>;		/* 0: single 1: dual */
		ports = <2 3>;

	};

	tcc-drm-lcd {
	        compatible = "telechips,tcc-drm-lcd";
	        display-port-num = <0>;
	        reg = <0x12000000 0x100 0x12000400 0x100 0x12001800 0x100>,
				/* VIOC_REMAP */
				  <0x12000000 0x100 0x12004000 0x100 0x12018000 0x100>;
	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD0>;
	        status = "okay";
		display-timings {
			native-mode = <&timing0>;
			timing0: timing {
				clock-frequency = <88200000>;
				hactive = <1920>;
				vactive = <720>;
				hfront-porch = <28>;
				hback-porch = <28>;
				hsync-len = <8>;
				vback-porch = <10>;
				vfront-porch = <10>;
				vsync-len = <2>;
			};
		};
	};

	tcc-drm-ext {
	        compatible = "telechips,tcc-drm-ext";
	        display-port-num = <1>;
	        reg = <0x12000100 0x100 0x12000800 0x100 0x12001900 0x100>,
				/* VIOC_REMAP */
				  <0x12001000 0x100 0x12008000 0x100 0x12019000 0x100>;
	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD1>;
	        status = "okay";
		display-timings {
			native-mode = <&timing1>;
			timing1: timing {
				clock-frequency = <88200000>;
				hactive = <1920>;
				vactive = <720>;
				hfront-porch = <28>;
				hback-porch = <28>;
				hsync-len = <8>;
				vback-porch = <10>;
				vfront-porch = <10>;
				vsync-len = <2>;
			};
		};
	};

	tcc-drm-third {
	        compatible = "telechips,tcc-drm-third";
	        display-port-num = <2>;
	        reg = <0x12000200 0x100 0x12000C00 0x100 0x12001A00 0x100>,
				/* VIOC_REMAP */
				  <0x12002000 0x100 0x1200C000 0x100 0x1201A000 0x100>;
	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD2>;
	        status = "okay";
		display-timings {
			native-mode = <&timing2>;
			timing2: timing {
				clock-frequency = <51200000>;
				hactive = <1024>;
				vactive = <600>;
				hfront-porch = <147>;
				hback-porch = <147>;
				hsync-len = <19>;
				vback-porch = <10>;
				vfront-porch = <25>;
				vsync-len = <2>;
			};
		};
	};

	vout_drv@1 {
	    rdma = <&vioc_rdma VIOC_RDMA10>;
	    wmix = <&vioc_wmix VIOC_WMIX2>;
	    disp = <&vioc_disp VIOC_DISP2>;
	    subplane_rdma = <&vioc_rdma VIOC_RDMA09>;
		status = "disabled";
	};

	vioc_mgr@0 {
		status = "okay";
	};
	tcc-gpio-sample {
		compatible = "gpio-sample";
		leble = "TCC GPIO";
		gpios = <&gpa 11 0>;
	};
};
