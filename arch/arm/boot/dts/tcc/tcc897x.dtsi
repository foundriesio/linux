#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/clock/tcc897x-clks.h>
#include <dt-bindings/soc/tcc897x-pm.h>
#include <dt-bindings/display/tcc897x-vioc.h>
#include <dt-bindings/power/telechips,boot-mode.h>

#include "tcc-pmap-common.dtsi"

/ {
	compatible = "telechips,tcc897x";
	interrupt-parent = <&gic>;
	#address-cells = <1>;
	#size-cells = <1>;

	chosen {
		bootargs = "vmalloc=480M console=ttyS0,115200n8";
	};

	memory@80000000 {
		#address-cells = <1>;
		#size-cells = <1>;
		device_type = "memory";
		reg = <0x80000000 0x80000000>;
	};

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
		serial5 = &uart5;
		serial6 = &uart6;
		serial7 = &uart7;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		sdhc0 = &sdhc0;
		sdhc2 = &sdhc2;
		sdhc3 = &sdhc3;
		ehci = &ehci;
		pcm0 = &pcm0;
		pcm1 = &pcm1;
		adma0 = &adma0;
		adma1 = &adma1;
		adma2 = &adma2;
		adma3 = &adma3;
		i2s0 = &i2s0;
		i2s1 = &i2s1;
		i2s2 = &i2s2;
		i2s3 = &i2s3;
		i2s10 = &i2s10;
		i2s11 = &i2s11;
		i2s12 = &i2s12;
		i2s13 = &i2s13;
		spdif0 = &spdif0;
		spdif1 = &spdif1;
		cdif0 = &cdif0;
		cdif1 = &cdif1;
		dma0 = &dma0;
		dma1 = &dma1;
		dma2 = &dma2;
		dma3 = &dma3;
		dma4 = &dma4;
		wmixer_drv0 = &wmixer_drv0;
		wmixer_drv1 = &wmixer_drv1;
		scaler_drv1 = &scaler_drv1;
		scaler_drv3 = &scaler_drv3;
		vout_drv0 = &vout_drv0;
		vout_drv1 = &vout_drv1;
		wdma_drv0 = &wdma_drv0;
		videoinput0 = &videoinput0;
		fb0 = &fb0;
		fb1 = &fb1;
		fb2 = &fb2;
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a7";
			reg = <0x0>;
			clock-frequency = <1000000000>;
			operating-points = <
				/* kHz    uV */
				1000000	1100000
				 925000	1050000
				 850000	1000000
				 775000	 950000
				 700000	 900000
			>;
		};
		cpu1: cpu@1 {
			device_type = "cpu";
			compatible = "arm,cortex-a7";
			reg = <0x1>;
			clock-frequency = <1000000000>;
		};
		cpu2: cpu@2 {
			device_type = "cpu";
			compatible = "arm,cortex-a7";
			reg = <0x2>;
			clock-frequency = <1000000000>;
		};
		cpu3: cpu@3 {
			device_type = "cpu";
			compatible = "arm,cortex-a7";
			reg = <0x3>;
			clock-frequency = <1000000000>;
		};
	};

	gic:interrupt-controller@77101000 {
		compatible = "arm,cortex-a7-gic", "telechips,tcc897x-vpic";
		#interrupt-cells = <3>;
		#address-cells = <0>;
		interrupt-controller;
		reg = <0x77101000 0x1000>,
		      <0x77102000 0x1000>,
		      <0x74100000 0x260>,
		      <0x7460000c 0x8>;
	};

	timer {
		compatible = "arm,armv7-timer";
		interrupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 14 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 11 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 10 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>;
		always-on;
	};

	pmu_a7 {
		compatible = "arm,cortex-a7-pmu";
		interrupts = <GIC_SPI 64 IRQ_TYPE_LEVEL_LOW>,
		             <GIC_SPI 65 IRQ_TYPE_LEVEL_LOW>,
		             <GIC_SPI 66 IRQ_TYPE_LEVEL_LOW>,
		             <GIC_SPI 67 IRQ_TYPE_LEVEL_LOW>;
	};

	clocks {
		compatible = "telechips,ckc";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x74000000 0x1A8	/* ckc */
		       0x74400000 0x114	/* pmu */
		       0x73810000 0x018	/* memory bus configutaion */
		       0x72380000 0x008	/* display bus configutaion */
		       0x70010000 0x008	/* graphic bus configutaion */
		       0x76066000 0x070	/* io bus configutaion */
		       0x75100000 0x008	/* video bus configutaion */
		       0x71EA0000 0x008	/* hsio bus configutaion */
		       >;
		ranges;

		osc24m: oscillator {
			compatible = "fixed-clock";
			#clock-cells = <0>;
			clock-frequency = <24000000>;
			clock-output-names = "osc24m";
		};

		clk_pll: pll_clk {
			compatible = "telechips,clk-pll";
			#clock-cells = <1>;
			clocks = <&osc24m>;
			clock-output-names = "pll0", "pll1", "pll2", "pll_cb",
					     "pll_mb";
		};

		clk_fbus: clk@74000000 {
			compatible = "telechips,clk-fbus";
			#clock-cells = <1>;
			clock-indices =
				<FBUS_CPU0>, <FBUS_CPU1>, <FBUS_MEM>, <FBUS_DDI>,
				<FBUS_GPU>, <FBUS_IO>, <FBUS_VBUS>, <FBUS_CODA>,
				<FBUS_HSIO>, <FBUS_SMU>, <FBUS_G2D>, <FBUS_CMBUS>,
				<FBUS_HEVC_VCE>, <FBUS_HEVC_VCPU>,
				<FBUS_HEVC_BPU>;
			clock-output-names =
				"fbus_cpu0", "fbus_cpu1", "fbus_mem", "fbus_ddi",
				"fbus_gpu", "fbus_io", "fbus_vbus", "fbus_coda",
				"fbus_hsio", "fbus_smu", "fbus_g2d", "fbus_cmbus",
				"fbus_hevc_vce", "fbus_hevc_vcpu",
				"fbus_hevc_bpu";
			telechips,clock-flags =
				<0>,	/* FBUS_CPU0 */
				<0>,	/* FBUS_CPU1 */
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* FBUS_MEM */
				<CLK_SET_RATE_GATE>,	/* FBUS_DDI */
				<0>,	/* FBUS_GPU */
				<(CLK_SET_RATE_GATE | CLK_IS_CRITICAL)>,	/* FBUS_IO */
				<CLK_SET_RATE_GATE>,	/* FBUS_VBUS */
				<CLK_SET_RATE_GATE>,	/* FBUS_CODA */
				<CLK_SET_RATE_GATE>,	/* FBUS_HSIO */
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* FBUS_SMU */
				<CLK_SET_RATE_GATE>,	/* FBUS_G2D */
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* FBUS_CMBUS */
				<CLK_SET_RATE_GATE>,	/* FBUS_HEVC_VCE */
				<CLK_SET_RATE_GATE>,	/* FBUS_HEVC_VCPU */
				<CLK_SET_RATE_GATE>;	/* FBUS_HEVC_BPU */
		};

		clk_peri: clk@74000080 {
			compatible = "telechips,clk-peri";
			#clock-cells = <1>;
			clock-indices =
				<PERI_TCX>, <PERI_TCT>, <PERI_TCZ>, <PERI_LCD0>,
				<PERI_LCDSI0>, <PERI_LCD1>, <PERI_LCDSI1>,
				<PERI_LCDTIMER>, <PERI_USB_OTG>,
				<PERI_GMAC_PTP>, <PERI_GMAC>, <PERI_USB_OTG_ADP>,
				<PERI_IOB_CEC>, <PERI_OUT0>, <PERI_HDMI>,
				<PERI_HDMIA>, <PERI_OUT1>, <PERI_REMOTE>,
				<PERI_SDMMC0>, <PERI_SDMMC1>, <PERI_SDMMC2>,
				<PERI_SDMMC3>, <PERI_ADAI3>, <PERI_ADAM3>,
				<PERI_SPDIF3>, <PERI_ADAI0>, <PERI_ADAM0>,
				<PERI_SPDIF0>, <PERI_PDM>, <PERI_CMBUS_CNT>,
				<PERI_ADC>, <PERI_I2C0>, <PERI_I2C1>, <PERI_I2C2>,
				<PERI_I2C3>, <PERI_UART0>, <PERI_UART1>,
				<PERI_UART2>, <PERI_UART3>, <PERI_UART4>,
				<PERI_UART5>, <PERI_UART6>, <PERI_UART7>,
				<PERI_GPSB0>, <PERI_GPSB1>, <PERI_GPSB2>,
				<PERI_I2C6>, <PERI_I2C7>, <PERI_OUT2>, <PERI_OUT3>,
				<PERI_OUT4>, <PERI_OUT5>, <PERI_TSTX0>,
				<PERI_CMBUS_TSIF0>, <PERI_CMBUS_TSIF1>,
				<PERI_CMBUS_TSIF2>, <PERI_CMBUS_TSIF3>, <PERI_I2C4>,
				<PERI_I2C5>, <PERI_HDMI_PCLK>, <PERI_ADAI1>,
				<PERI_ADAM1>, <PERI_SPDIF1>, <PERI_ADAI2>,
				<PERI_ADAM2>, <PERI_SPDIF2>;
			clock-output-names =
				"peri_tcx", "peri_tct", "peri_tcz", "peri_lcd0",
				"peri_lcdsi0", "peri_lcd1", "peri_lcdsi1",
				"peri_lcdtimer", "peri_usb_otg",
				"peri_gmac_ptp", "peri_gmac", "peri_usb_otg_adp",
				"peri_iob_cec", "peri_out0", "peri_hdmi",
				"peri_hdmia", "peri_out1", "peri_remote",
				"peri_sdmmc0", "peri_sdmmc1", "peri_sdmmc2",
				"peri_sdmmc3", "peri_adai3", "peri_adam3",
				"peri_spdif3", "peri_adai0", "peri_adam0",
				"peri_spdif0", "peri_pdm", "peri_cmbus_cnt",
				"peri_adc", "peri_i2c0", "peri_i2c1", "peri_i2c2",
				"peri_i2c3", "peri_uart0", "peri_uart1",
				"peri_uart2", "peri_uart3", "peri_uart4",
				"peri_uart5", "peri_uart6", "peri_uart7",
				"peri_gpsb0", "peri_gpsb1", "peri_gpsb2",
				"peri_i2c6", "peri_i2c7", "peri_out2", "peri_out3",
				"peri_out4", "peri_out5", "peri_tstx0",
				"peri_cmbus_tsif0", "peri_cmbus_tsif1",
				"peri_cmbus_tsif2", "peri_cmbus_tsif3", "peri_i2c4",
				"peri_i2c5", "peri_hdmi_pclk", "peri_adai1",
				"peri_adam1", "peri_spdif1", "peri_adai2",
				"peri_adam2", "peri_spdif2";
			telechips,clock-flags =
				<0>,	/* PERI_TCX */
				<0>,	/* PERI_TCT */
				<CLK_IGNORE_UNUSED>,	/* PERI_TCZ */
				<0>,	/* PERI_LCD0 */
				<0>,	/* PERI_LCDSI0 */
				<0>,	/* PERI_LCD1 */
				<0>,	/* PERI_LCDSI1 */
				<0>,	/* PERI_LCDTIMER */
				<0>,	/* PERI_USB_OTG */
				<0>,	/* PERI_GMAC_PTP */
				<0>,	/* PERI_GMAC */
				<0>,	/* PERI_USB_OTG_ADP */
				<0>,	/* PERI_IOB_CEC */
				<0>,	/* PERI_OUT0 */
				<0>,	/* PERI_HDMI */
				<0>,	/* PERI_HDMIA */
				<0>,	/* PERI_OUT1 */
				<0>,	/* PERI_REMOTE */
				<0>,	/* PERI_SDMMC0 */
				<0>,	/* PERI_SDMMC1 */
				<0>,	/* PERI_SDMMC2 */
				<0>,	/* PERI_SDMMC3 */
				<0>,	/* PERI_ADAI3 */
				<0>,	/* PERI_ADAM3 */
				<0>,	/* PERI_SPDIF3 */
				<0>,	/* PERI_ADAI0 */
				<0>,	/* PERI_ADAM0 */
				<0>,	/* PERI_SPDIF0 */
				<0>,	/* PERI_PDM */
				<CLK_IGNORE_UNUSED>,	/* PERI_CMBUS_CNT */
				<0>,	/* PERI_ADC */
				<0>,	/* PERI_I2C0 */
				<0>,	/* PERI_I2C1 */
				<0>,	/* PERI_I2C2 */
				<0>,	/* PERI_I2C3 */
				<0>,	/* PERI_UART0 */
				<0>,	/* PERI_UART1 */
				<0>,	/* PERI_UART2 */
				<0>,	/* PERI_UART3 */
				<0>,	/* PERI_UART4 */
				<0>,	/* PERI_UART5 */
				<0>,	/* PERI_UART6 */
				<0>,	/* PERI_UART7 */
				<0>,	/* PERI_GPSB0 */
				<0>,	/* PERI_GPSB1 */
				<0>,	/* PERI_GPSB2 */
				<0>,	/* PERI_I2C6 */
				<0>,	/* PERI_I2C7 */
				<0>,	/* PERI_OUT2 */
				<0>,	/* PERI_OUT3 */
				<0>,	/* PERI_OUT4 */
				<0>,	/* PERI_OUT5 */
				<0>,	/* PERI_TSTX0 */
				<0>,	/* PERI_CMBUS_TSIF0 */
				<0>,	/* PERI_CMBUS_TSIF1 */
				<0>,	/* PERI_CMBUS_TSIF2 */
				<0>,	/* PERI_CMBUS_TSIF3 */
				<0>,	/* PERI_I2C4 */
				<0>,	/* PERI_I2C5 */
				<0>,	/* PERI_HDMI_PCLK */
				<CLK_IGNORE_UNUSED>,	/* PERI_ADAI1 */
				<CLK_IGNORE_UNUSED>,	/* PERI_ADAM1 */
				<0>,	/* PERI_SPDIF1 */
				<0>,	/* PERI_ADAI2 */
				<0>,	/* PERI_ADAM2 */
				<0>;	/* PERI_SPDIF2 */
		};

		clk_isoip_top: clk@7440009c {
			compatible = "telechips,clk-isoip_top";
			#clock-cells = <1>;
			clock-indices =
				<ISOIP_TOP_OTP>, <ISOIP_TOP_RTC>,
				<ISOIP_TOP_PLL>, <ISOIP_TOP_ECID>,
				<ISOIP_TOP_HDMI>, <ISOIP_TOP_VDAC_3CH>,
				<ISOIP_TOP_VDAC_1CH>, <ISOIP_TOP_TSADC>,
				<ISOIP_TOP_USB2P>, <ISOIP_TOP_USBOTG>,
				<ISOIP_TOP_LVDS>;
			clock-output-names =
				"isoip_top_otp", "isoip_top_rtc",
				"isoip_top_pll", "isoip_top_ecid",
				"isoip_top_hdmi", "isoip_top_vdac_3ch",
				"isoip_top_vdac_1ch", "isoip_top_tsadc",
				"isoip_top_usb2p", "isoip_top_usbotg",
				"isoip_top_lvds";
			telechips,clock-flags =
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* ISOIP_TOP_OTP */
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* ISOIP_TOP_RTC */
				<(CLK_SET_RATE_GATE | CLK_IGNORE_UNUSED)>,	/* ISOIP_TOP_PLL */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_ECID */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_HDMI */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_VDAC_3CH */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_VDAC_1CH */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_TSADC */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_USB2P */
				<CLK_SET_RATE_GATE>,	/* ISOIP_TOP_USBOTG */
				<CLK_SET_RATE_GATE>;	/* ISOIP_TOP_LVDS */
		};

		clk_ddi: clk@72380000 {
			compatible = "telechips,clk-ddibus";
			#clock-cells = <1>;
			clock-indices =
				<DDIBUS_VIOC>, <DDIBUS_NTSCPAL>, <DDIBUS_HDMI>,
				<DDIBUS_G2D>;
			clock-output-names =
				"ddibus_vioc", "ddibus_ntscpal", "ddibus_hdmi",
				"ddibus_g2d";
			telechips,clock-flags =
				<CLK_IGNORE_UNUSED>,	/* DDIBUS_VIOC */
				<CLK_IGNORE_UNUSED>,	/* DDIBUS_NTSCPAL */
				<CLK_IGNORE_UNUSED>,	/* DDIBUS_HDMI */
				<CLK_IGNORE_UNUSED>;	/* DDIBUS_G2D */
		};

		clk_io: clk@76066000 {
			compatible = "telechips,clk-iobus";
			#clock-cells = <1>;
			clock-indices =
				<IOBUS_SDMMC0>, <IOBUS_SDMMC2>, <IOBUS_SDMMC3>,
				<IOBUS_SDMMC>, <IOBUS_DMA0>, <IOBUS_DMA1>,
				<IOBUS_DMA2>, <IOBUS_DMA>, <IOBUS_PWM>, <IOBUS_SMC>,
				<IOBUS_I2C_S2>, <IOBUS_RTC>, <IOBUS_REMOCON>,
				<IOBUS_TSADC>, <IOBUS_EDICFG>, <IOBUS_EDI>,
				<IOBUS_PROT>, <IOBUS_ADMA0>, <IOBUS_DAI0>,
				<IOBUS_SPDIF0>, <IOBUS_AUDIO0>, <IOBUS_ADMA3>,
				<IOBUS_DAI3>, <IOBUS_SPDIF3>, <IOBUS_AUDIO3>,
				<IOBUS_I2C_M0>, <IOBUS_I2C_M1>, <IOBUS_I2C_M2>,
				<IOBUS_I2C_M3>, <IOBUS_I2C_S0>, <IOBUS_I2C_S1>,
				<IOBUS_I2C>, <IOBUS_UART0>, <IOBUS_UART1>,
				<IOBUS_UART2>, <IOBUS_UART3>, <IOBUS_UART4>,
				<IOBUS_UART5>, <IOBUS_UART6>, <IOBUS_UART7>,
				<IOBUS_UART>, <IOBUS_NFC>, <IOBUS_TS0>, <IOBUS_GPSB0>,
				<IOBUS_GPSB1>, <IOBUS_GPSB2>, <IOBUS_GPSB>,
				<IOBUS_CEC>, <IOBUS_I2C_S3>, <IOBUS_I2C_M4>,
				<IOBUS_I2C_M5>, <IOBUS_I2C_M6>, <IOBUS_I2C_M7>,
				<IOBUS_DMAC0>, <IOBUS_DMAC1>, <IOBUS_DMAC2>,
				<IOBUS_DMAC>, <IOBUS_ADMA1>, <IOBUS_DAI1>,
				<IOBUS_SPDIF1>, <IOBUS_AUDIO1>, <IOBUS_ADMA2>,
				<IOBUS_DAI2>, <IOBUS_SPDIF2>, <IOBUS_AUDIO2>;
			clock-output-names =
				"iobus_sdmmc0", "iobus_sdmmc2", "iobus_sdmmc3",
				"iobus_sdmmc", "iobus_dma0", "iobus_dma1",
				"iobus_dma2", "iobus_dma", "iobus_pwm", "iobus_smc",
				"iobus_i2c_s2", "iobus_rtc", "iobus_remocon",
				"iobus_tsadc", "iobus_edicfg", "iobus_edi",
				"iobus_prot", "iobus_adma0", "iobus_dai0",
				"iobus_spdif0", "iobus_audio0", "iobus_adma3",
				"iobus_dai3", "iobus_spdif3", "iobus_audio3",
				"iobus_i2c_m0", "iobus_i2c_m1", "iobus_i2c_m2",
				"iobus_i2c_m3", "iobus_i2c_s0", "iobus_i2c_s1",
				"iobus_i2c", "iobus_uart0", "iobus_uart1",
				"iobus_uart2", "iobus_uart3", "iobus_uart4",
				"iobus_uart5", "iobus_uart6", "iobus_uart7",
				"iobus_uart", "iobus_nfc", "iobus_ts0", "iobus_gpsb0",
				"iobus_gpsb1", "iobus_gpsb2", "iobus_gpsb",
				"iobus_cec", "iobus_i2c_s3", "iobus_i2c_m4",
				"iobus_i2c_m5", "iobus_i2c_m6", "iobus_i2c_m7",
				"iobus_dmac0", "iobus_dmac1", "iobus_dmac2",
				"iobus_dmac", "iobus_adma1", "iobus_dai1",
				"iobus_spdif1", "iobus_audio1", "iobus_adma2",
				"iobus_dai2", "iobus_spdif2", "iobus_audio2";
			telechips,clock-flags =
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SDMMC0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SDMMC2 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SDMMC3 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SDMMC */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DMA0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DMA1 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DMA2 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DMA */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_PWM */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SMC */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_I2C_S2 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_RTC */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_REMOCON */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_TSADC */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_EDICFG */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_EDI */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_PROT */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_ADMA0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DAI0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SPDIF0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_AUDIO0 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_ADMA3 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_DAI3 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_SPDIF3 */
				<CLK_IGNORE_UNUSED>,	/* IOBUS_AUDIO3 */
