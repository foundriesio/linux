	/* CP */
	auth-cp {
		compatible = "telechips, tcc-cp";
		cp-type = <2>;		/* 2.0C = 1, 2.0B = 0, 3.0 = 2 */
		i2c-channel = <3>;	/* 0 ~ 3 */
		/*	power-ctrl-able; */
		/* cp_power-gpio = <&gpk 14 0>; */
		cp_reset-gpio = <&gpa 11 0>;
	};

	/* eMMC */
	vqmmc_emmc: vqmmc_emmc {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_emmc";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	sdhc@16440000 {
		status = "okay";
		bus-width = <8>;
		max-frequency = <200000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus8>;

		non-removable;
		keep-power-in-suspend;
		disable-wp;
		cap-mmc-hw-reset;
		mmc-ddr-1_8v;

		tcc-mmc-reset = <&gpsd0 14 0>;

		vqmmc-supply = <&vqmmc_emmc>;
	};

	/* SD CARD */
	sdhc@16450000 {
		status = "okay";
		/*
		 * In order to reduce boot time,
		 * we use the driver as module not built-in.
		 */
		compatible = "telechips,tcc803x-sdhci,module-only";
		bus-width = <4>;
		max-frequency = <200000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus4 &sd1_bus4_wp_cd>;
	};

