		txclk-i-inv = <0>;
		txen-dly = <0>;
		txer-dly = <0>;
		txd0-dly = <0>;
		txd1-dly = <0>;
		txd2-dly = <0>;
		txd3-dly = <0>;
		txd4-dly = <0>;
		txd5-dly = <0>;
		txd6-dly = <0>;
		txd7-dly = <0>;
		rxclk-i-dly = <0>;
		rxclk-i-inv = <0>;
		rxdv-dly = <0>;
		rxer-dly = <0>;
		rxd0-dly = <0>;
		rxd1-dly = <0>;
