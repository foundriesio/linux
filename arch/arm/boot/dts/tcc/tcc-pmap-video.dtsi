	#define INST_3RD_PROC_SIZE 				(0)
	#define INST_4TH_PROC_SIZE 				(0)
	#define SUPPORT_HEVC_OF_SUB_INSTANCE 	(0)
	#define SUPPORT_VPU_OF_SUB_INSTANCE 	(0)
#endif

