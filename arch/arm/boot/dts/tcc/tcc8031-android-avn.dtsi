&i2c0 {
	status = "okay";
	port-mux = <12>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c12_bus>;

	pmic@5A {
		compatible = "dlg,da9063";
		reg = <0x5A>;

		da9063_gpio: gpio {
			compatible = "dlg,da9063-gpio";
			gpio-controller;
			#gpio-cells = <2>;
		};

		regulators {
			vdd_bcore1: bcore1 {
				regulator-name = "cpu_0p9v";
				regulator-min-microvolt = <855000>;
				regulator-max-microvolt = <900000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_bcore2: bcore2 {
				regulator-name = "core_0p8v";
				regulator-min-microvolt = <760000>;
				regulator-max-microvolt = <800000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_bpro: bpro {
				regulator-name = "vdd_mali";
				regulator-min-microvolt = <760000>;
				regulator-max-microvolt = <800000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_bmem: bmem {
				regulator-name = "io_1p8v";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_bio: bio {
				regulator-name = "io_3p3v";
				regulator-min-microvolt = <3000000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_bperi: bperi {
				regulator-name = "mem_1p1v";
				regulator-min-microvolt = <1045000>;
				regulator-max-microvolt = <1100000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_ldo3: ldo3 {
				/* peri_1p8v */
				regulator-name = "sdio_pwr";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_ldo7: ldo7 {
				regulator-name = "cpu_1p2v";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <1200000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_ldo8: ldo8 {
				regulator-name = "mem_1p8v";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_ldo9: ldo9 {
				regulator-name = "pmic_io_pwr";
				regulator-min-microvolt = <950000>;
				regulator-max-microvolt = <3600000>;
				regulator-always-on;
				regulator-boot-on;
			};

			vdd_ldo11: ldo11 {
				/* peri_3p3v */
				regulator-name = "emmc_pwr";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;
			};
		};
	}; /* pmic da9063 */

	videosource0: videodecoder_isl79988 {
		compatible	= "intersil,isl79988";
		pinctrl-names	= "idle", "active";
		cifport		= <0>;
		pinctrl-0	= <&cam0_8bit_idle>;
		pinctrl-1	= <&cam0_clk &cam0_data_8bit>;
		rst-gpios	= <&gpb 14 1>;
		reg		= <0x44>;	// 0x88 >> 1
	};

	videosource1: deserializer_max9286 {
		compatible	= "maxim,max9286";
		cifport		= <5>;
		pwd-gpios	= <&gpg 5 1>;
		intb-gpios	= <&gph 11 1>;
		reg		= <0x48>;	// 0x90 >> 1
		mipi-csi2-idx   = <0>;
	};
};

&i2c1 {
	status = "okay";
	port-mux = <13>; /* [0]SCL [1]SDA */
	pinctrl-names = "default";
	pinctrl-0 = <&i2c13_bus>;

	ssd253x@48 {
		compatible = "solomon,ssd253x";
		reg = <0x48>;
		pinctrl-names = "default";
		pinctrl-0 = <&tsc_default>;
		int-gpios = <&gpsd1 9 0x0>;
		rst-gpios = <&gpb 16 0x0>;
	};
	mxt_touch@4a {
		compatible = "atmel,maxtouch";
		reg = <0x4a>;
		pinctrl-names = "default";
		pinctrl-0 = <&tsc_default>;

		irq-gpios = <&gpsd1 9 0x0>;
		reset-gpios = <&gpb 16 0x0>;
	};
	ga657x@5d {
		compatible = "goodix,ga657x";
		reg = <0x5d>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&tsc_default>;

		reset-gpios = <&gpb 16 0x0>;
		irq-gpios = <&gpsd1 9 0x0>;
		irq-flags = <2>;

		touchscreen-max-id = <11>;
		touchscreen-size-x = <1920>;
		touchscreen-size-y = <720>;
		touchscreen-max-w = <512>;
		touchscreen-max-p = <512>;
		touchscreen-key-map = <172>, <158>; /*KEY_HOMEPAGE=172, KEY_BACK=158, KEY_MENU=139*/
		goodix,slide-wakeup = <0>;
		goodix,type-a-report = <0>;
		goodix,driver-send-cfg = <0>;
		goodix,resume-in-workqueue = <0>;
		goodix,swap-x2y = <0>;
		goodix,esd-protect = <1>;
		goodix,auto-update = <0>;
		goodix,auto-update-cfg = <0>;
		goodix,power-off-sleep = <0>;
		goodix,pen-suppress-finger = <0>;
		goodix,cfg-group0 = [
			53 D0 02 00 05 05 F5 D5 21 48 2D 0F 5A 41 0E 05 00 00 32 32 20 00 05 14 14 1A 14 8B 2B 00
		];
	};
};

&i2c2 {
	status = "okay";
	port-mux = <20>; /* [0]SCL [1]SDA */
