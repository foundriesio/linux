/ {
	model = "Telechips TCC8059 Evaluation Board for AVN";
	compatible = "telechips,tcc8059", "telechips,tcc805x";

	aux_detect {
		compatible = "telechips,aux_detect";
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&aux_detect_ma14>;
		aux-gpios = <&gpma 14 1>;
		aux-active = <1>;
	};
};

&aliases {
	videosource0 = &videosource0;
	videosource1 = &videosource1;
	videosource2 = &videosource2;
};

&pcie {
	reset-gpio = <&gpc 30 0>;
	//status = "okay";
};

