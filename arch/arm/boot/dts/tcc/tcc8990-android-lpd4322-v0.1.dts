&tcm3800 {
	compatible = "telechips, tcm3800";
	wifi_pwr-gpio = <&gpsd1 10 0>;
	wlreg_on-gpio = <&gpe 15 0>;
	wifi_pwr_1p8v-gpio = <&gpsd1 6 0>;
	wifi_pwr_3p3v-gpio = <&gpsd1 7 0>;
};
