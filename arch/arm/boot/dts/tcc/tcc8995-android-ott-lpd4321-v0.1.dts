&tcc_bluetooth {
	compatible = "telechips, tcc_bluetooth";
	bt_power-gpio = <&gpf 30 0>; /* BTWIFI_VBAT_EN */
	bt_reg_on-gpio = <&gpc 21 0>;
	bt_wake-gpio = <&gpc 20 0>;
	bt_hwake-gpio = <&gpc 22 0>;
	bt_power_1p8v-gpio = <&gpg 4 0>;
	status = "okay";
};

&gpio_keys {
	compatible = "gpio-keys";
	#address-cells = <1>;
	#size-cells = <0>;
	autorepeat;
	button@0 {
		label = "Power Key";
		linux,code = <116>;
		gpios = <&gpd 20 1>;
	};
};
