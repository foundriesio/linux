/* USB 2.0 OTG */
&dwc2 {
	status = "okay";
	phy-type = <1>;		//pico phy = 1, nano phy = 0

	vbus-ctrl-able;
	vbus-gpio = <&gpd 18 0>;
};

&dwc_otg {
	status = "okay";
	phy-type = <1>;     //pico phy = 1, nano phy = 0

	vbus-ctrl-able;
	vbus-gpio = <&gpd 18 0>;
};

&audio_chmux {
	compatible = "telechips,audio-chmux-899x";
	dai = <99 1>; // i2s0(x) i2s1(x)
	cdif = <99 99>; // cdif0(x) cdif1(x)
	spdif = <1 99>; // spdif0(x) spdif1(x)
	status = "okay";
};

&i2s0 {
	pinctrl-names = "default", "idle";
	pinctrl-0 = <&m0dai1_clks &m0dai1_dao0 &m0dai1_dai0>;
	pinctrl-1 = <&m0dai1_clks_idle &m0dai1_dao0_idle &m0dai1_dai0_idle>;
	status = "disable";
};

&i2s1 {
	pinctrl-names = "default", "idle";
	pinctrl-0 = <&m1dai1_clks &m1dai1_dao0>;
	pinctrl-1 = <&m1dai1_clks_idle &m1dai1_dao0_idle>;
	status = "okay";
};

&spdif0	{
	pinctrl-names = "default", "idle";
	pinctrl-0 = <&m0dai1_spdif_tx>;
	pinctrl-1 = <&m0dai1_spdif_tx_idle>;	
	tx-only;
	status = "okay";
};

&spdif1	{
	pinctrl-names = "default", "idle";
	pinctrl-0 = <&m1dai1_spdif_tx>;
	pinctrl-1 = <&m1dai1_spdif_tx_idle>;
	tx-only;
	status = "disable";
};

&adma0 {
	status = "okay";
};

&adma1 {
	status = "okay";
};

&wm8524 {
	compatible = "wolfson,wm8524";
	status = "okay";

	/*codec_aif-gpios = <&gpd 6 0>;*/
	codec_mute_ang-gpios = <&gpg 19 0>;
	codec_mute_ctl-gpios = <&gpg 18 0>;
	/* value to enable mute*/
	codec_mute_ang-mute_value = <0>;
};

&sound {
	compatible = "telechips,snd-card";
	card-name = "TCC8990 STB Card";
	status = "okay";

	telechips,dai-link@0 {
		stream-name="I2S_1";

		codec,format = "i2s"; // i2s, left_j, right_j
		codec,continuous-clock;

		mclk_div = <4>; // 4,6,8,16
		bclk_ratio = <64>; // 32,48,64fs
		pcm = <&adma1>;
		dai = <&i2s1>;
		codec = <&wm8524>;
		codec,dai-name = "wm8524-hifi";
	};

	telechips,dai-link@1 {
		stream-name="SPDIF_0_TX";

		pcm = <&adma0>;
		dai = <&spdif0>;
	};
};

&remote {
	int-gpios = <&gpc 28 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&remote_c28>;
	wakeupsrc = <011>; //RMISEL is 0x3 : GPIO_C28
	status = "okay";
};

