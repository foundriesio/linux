#ifndef DT_OVERLAY
/dts-v1/;
#endif

#include "tcc8059-android-avn.dtsi"


/ {
	/* Describe here after remove this statement if you have something to be override. */
	tm123xdhp90 {
		compatible = "telechips,lvds-tm123xdhp90";
		status = "okay";
		lvds0 = <&lvds_phy>;
		power-on-gpios = <&gpc 8 0>;   // LCD_ON
		reset-gpios = <&gpc 9 0>;    // LCD_RST
		/*
			 display-on-gpios = ;    // LCD_DISP
			 lvds-stby-gpios = ;   // LCD_DE = LVDS_STBY in LVDS
			 lvds-power-gpios = ;    // LVDS_EN
			 lvds-power-gpios1 = ;   // V_5V0_EN
		 */
		mode = <1>;   /* 0: single 1: dual */
		ports = <2 3>;
		mux-select = <2>;
	};

	tcc-drm-lcd {
	        compatible = "telechips,tcc-drm-lcd";
	        display-port-num = <0>;
	        reg = <0x0 0x12000000 0x0 0x100>,
		      <0x0 0x12000400 0x0 0x100>,
		      <0x0 0x12001800 0x0 0x100>,
		/* VIOC_REMAP */
		      <0x0 0x12000000 0x0 0x100>,
		      <0x0 0x12004000 0x0 0x100>,
		      <0x0 0x12018000 0x0 0x100>;

	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD0>;
	        status = "okay";
		display-timings {
			native-mode = <&timing0>;
			timing0: timing {
				clock-frequency = <88200000>;
				hactive = <1920>;
				vactive = <720>;
				hfront-porch = <28>;
				hback-porch = <28>;
				hsync-len = <8>;
				vback-porch = <10>;
				vfront-porch = <10>;
				vsync-len = <2>;
			};
		};
	};

	tcc-drm-ext {
	        compatible = "telechips,tcc-drm-ext";
	        display-port-num = <1>;
		reg = <0x0 0x12000100 0x0 0x100>,
		      <0x0 0x12000800 0x0 0x100>,
		      <0x0 0x12001900 0x0 0x100>,
		/* VIOC_REMAP */
		      <0x0 0x12001000 0x0 0x100>,
		      <0x0 0x12008000 0x0 0x100>,
		      <0x0 0x12019000 0x0 0x100>;
	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD1>;
	        status = "okay";
		display-timings {
			native-mode = <&timing1>;
			timing1: timing {
				clock-frequency = <88200000>;
				hactive = <1920>;
				vactive = <720>;
				hfront-porch = <28>;
				hback-porch = <28>;
				hsync-len = <8>;
				vback-porch = <10>;
				vfront-porch = <10>;
				vsync-len = <2>;
			};
		};
	};

	tcc-drm-third {
	        compatible = "telechips,tcc-drm-third";
	        display-port-num = <2>;
		reg = <0x0 0x12000200 0x0 0x100>,
		      <0x0 0x12000C00 0x0 0x100>,
		      <0x0 0x12001A00 0x0 0x100>,
		/* VIOC_REMAP */
		      <0x0 0x12002000 0x0 0x100>,
		      <0x0 0x1200C000 0x0 0x100>,
		      <0x0 0x1201A000 0x0 0x100>;
	        reg-names = "ddc", "rdma", "wmix";
	        interrupts = <GIC_SPI VIOC1_IRQI IRQ_TYPE_LEVEL_HIGH>;
	        interrupt-names = "vsync";
	        clock-names = "ddibus_vioc", "peri_lcd";
	        clocks = <&clk_ddi DDIBUS_VIOC>, <&clk_peri PERI_LCD2>;
	        status = "okay";
		display-timings {
			native-mode = <&timing2>;
			timing2: timing {
				clock-frequency = <51200000>;
				hactive = <1024>;
				vactive = <600>;
				hfront-porch = <147>;
				hback-porch = <147>;
				hsync-len = <19>;
				vback-porch = <10>;
				vfront-porch = <25>;
				vsync-len = <2>;
			};
		};
	};

	vqmmc_emmc: vqmmc_emmc {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_emmc";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	sdhc1_pwrseq: sdhc1_pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpsd1 8 GPIO_ACTIVE_LOW>;
	};

	vqmmc_sdslot: vqmmc_sdslot {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_sdslot";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	vqmmc_sdio: vqmmc_sdio {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_sdio";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};
};

/* eMMC */
&sdhc0 {
	status = "okay";
	bus-width = <8>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus8 &sd0_strb>;

	sdhci-caps-mask = <0x7 0x0>;

	non-removable;
	keep-power-in-suspend;
	disable-wp;
	cap-mmc-hw-reset;

	mmc-hs400-1_8v;
	tcc-mmc-taps = <0xF 0xF 0xF 0xF>;
	tcc-mmc-hs400-pos-tap = <0x6>;
	tcc-mmc-hs400-neg-tap = <0xB>;

	tcc-mmc-reset = <&gpsd0 14 0>;

	vqmmc-supply = <&vqmmc_emmc>;
};

&sd0_clk {
	telechips,no-pull;
};

&sd0_cmd {
	telechips,no-pull;
};

&sd0_bus8 {
	telechips,no-pull;
};

/* SD slot */
&sdhc1 {
#ifdef CONFIG_MMC_SDHCI_TCC_MODULE_ONLY
	/*
	 * In order to reduce boot time,
	 * we use the driver as module not built-in.
	 */
	compatible = "telechips,tcc805x-sdhci,module-only";
#endif
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus4 &sd1_bus4_wp_cd>;

	broken-cd;
	cd-inverted;
	keep-power-in-suspend;

	wp-gpios = <&gpsd1 7 0>;
	cd-gpios = <&gpsd1 6 0>;

	mmc-pwrseq = <&sdhc1_pwrseq>;
	vqmmc-supply = <&vqmmc_sdslot>;
};

&sd1_clk {
	telechips,no-pull;
};

&sd1_cmd {
	telechips,no-pull;
};

&sd1_bus8 {
	telechips,no-pull;
};

/* SDIO */
&sdhc2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus4 &out0_clk>;

	keep-power-in-suspend;
	cap-sdio-irq;

	vqmmc-supply = <&vqmmc_sdio>;

	cd-force-presence-change;

	tcm38xx {
		compatible = "telechips,tcm38xx";
		wifi_pwr-gpio = <&gpsd2 6 0>; /* BT_WIFI_VBAT_EN */
		wlreg_on-gpio = <&gpsd2 8 0>; /* WIFI_REG_ON */
		//wifi_pwr_1p8v-gpio = <&gpsd1 6 0>;
		//wifi_pwr_3p3v-gpio = <&gpsd1 7 0>;
	};
};

&sd2_clk {
	telechips,pull-up;
};

&sd2_cmd {
	telechips,pull-up;
};

&sd2_bus8 {
	telechips,pull-up;
};

&vioc_mgr0 {
        status = "okay";
};
