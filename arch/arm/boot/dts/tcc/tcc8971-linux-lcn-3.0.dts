#include "tcc8971.dtsi"

/ {
	model = "Telechips TCC8971 LCN Board SV3.0";
	compatible = "telechips,tcc8971", "telechips,tcc897x";

	/*tcc_bt_dev {
		compatible = "telechips, tcc_bt_dev";
		bt_reset-gpio = <&gpsd 10 0>;
	};*/
	/* cranking_time */
	/*cranking_detect {
