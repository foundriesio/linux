		compatible = "telechips, gps-gpio";
		gps_power-gpios = <&gpc 25 0>;
	};*/

	/* CP */
	auth-cp {
		compatible = "telechips, tcc-cp";
		cp-type = <1>;	    // 2.0C = 1, 2.0B = 0
		i2c-channel = <2>;	// 0 ~ 3
		//power-ctrl-able;
		//cp_power-gpio = <&gpg 19 0>;
		cp_reset-gpio = <&gpg 19 0>;
	};

