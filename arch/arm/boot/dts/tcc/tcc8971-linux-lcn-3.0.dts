	i2c@76300000 {
		status = "okay";
		port-mux = <11>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c11_bus>;

		ssd253x@48 {
			compatible = "solomon,ssd253x";
			reg = <0x48>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpio = <&gpd 21 0>;
			reset-gpio = <&gpe 5 0>;
		};
	};

	i2c@76310000 {
		status = "okay";
		port-mux = <18>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c18_bus>;

		/* video decoder using parallel interface output */
		adv7182: adv7182 {
			status		= "okay";
			compatible	= "adi,adv7182";
			pinctrl-names	= "default";
			pinctrl-0	= <&cam4_clk &cam4_data>;
			rst-gpios	= <&gpe  4 1>;
			reg		= <0x21>;	// 0x42 >> 1
			port {
				adv7182_out: endpoint {
					remote-endpoint = <&videoinput0_in>;
					io-direction	= "output";
					bus-width	= <8>;
				};
			};
		};
	};

	i2c@76320000 {
		status = "okay";
		/*Set CP I2C frequency to 100Khz*/
		scl-clock-frequency = <100000>;
		port-mux = <23>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c23_bus>;
	};

	i2c@76330000 {
		status = "okay";
		/*Set AK4601 I2C frequency to 400Khz.*/
		/*80Khz is okay, but too slow and make delay.*/
		scl-clock-frequency = <400000>;
		port-mux = <12>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c12_bus>;

		ak4601: ak4601@10 {
			compatible = "akm,ak4601";
			//ak4601,pdn-gpio = <&gpe 4 1>;
			cmute-gpios = <&gpc 18 1>;     //CODEC_MUTE    0: active high, 1: active low
			//amute-gpios = <&gpc 18 0>;      //AMP_MUTE      0: active high, 1: active low
			//stanby-gpios = <&gpc 16 0>;     //AMP_STBY      0: active high, 1: active low
			reg = <0x10>;
		};

		pmic: rt5028@3f {
			compatible = "Richtek,rt5028";
			reg = <0x3f>;
			int-gpios = <&gpe 31 0>;

			regulators {
				RT5028_ID_BUCK1: buck1 {
					regulator-name = "vdd_core";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK2: buck2 {
					regulator-name = "vdd_mem";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK3: buck3 {
					regulator-name = "vdd_cpu";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK4: buck4 {
					regulator-name = "vdd_io3v3";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO1: ldo1 {
					regulator-name = "vd2_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO2: ldo2 {
					regulator-name = "vdd_io1v8";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO3: ldo3 {
					regulator-name = "vdd_btwifi";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO4: ldo4 {
					regulator-name = "vdd_vbat";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO5: ldo5 {
					regulator-name = "vdd_gps";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO6: ldo6 {
					regulator-name = "vd1_io";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO7: ldo7 {
					regulator-name = "vd1_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO8: ldo8 {
					regulator-name = "vd2_io";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
			};
		};
	};

