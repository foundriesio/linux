	i2c@76300000 {
		status = "okay";
		port-mux = <11>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c11_bus>;

		ilitek@41 {
			compatible = "ilitek,aimvF";
			reg = <0x41>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		gt813@5d {
			compatible = "goodix,gt813";
			reg = <0x5d>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		vtl_ct26x@01 {
			compatible = "vtl,ct36x";
			reg = <0x01>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};

		ssd253x@48 {
			compatible = "solomon,ssd253x";
			reg = <0x48>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 21 0>;
			rst-gpios = <&gpe 5 0>;
		};
	};

	i2c@76310000 {
		status = "okay";
		port-mux = <18>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c18_bus>;

		videosource@adv7182 {
			compatible = "videosource,adv7182";
			reg = <0x21>;	// 0x42 >> 1
		};

		videosource@dm5886 {
			compatible = "videosource,dm5886";
			reg = <0x60>;	// 0xC0 >> 1
		};
	};

	i2c@76320000 {
		status = "okay";
		/*Set CP I2C frequency to 100Khz*/
		scl-clock-frequency = <100000>;
		port-mux = <23>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c23_bus>;
	};

	i2c@76330000 {
		status = "okay";
		/*Set AK4601 I2C frequency to 400Khz.*/
		/*80Khz is okay, but too slow and make delay.*/
		scl-clock-frequency = <400000>;
		port-mux = <12>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c12_bus>;

		ak4601: ak4601@10 {
			compatible = "akm,ak4601";
			//ak4601,pdn-gpio = <&gpe 4 1>;
			reg = <0x10>;
		};

		pmic: rt5028@3f {
			compatible = "Richtek,rt5028";
			reg = <0x3f>;
			int-gpios = <&gpe 31 0>;

			regulators {
				RT5028_ID_BUCK1: buck1 {
					regulator-name = "vdd_core";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK2: buck2 {
					regulator-name = "vdd_mem";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK3: buck3 {
					regulator-name = "vdd_cpu";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_BUCK4: buck4 {
					regulator-name = "vdd_io3v3";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO1: ldo1 {
					regulator-name = "vd2_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO2: ldo2 {
					regulator-name = "vdd_io1v8";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO3: ldo3 {
					regulator-name = "vdd_btwifi";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO4: ldo4 {
					regulator-name = "vdd_vbat";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO5: ldo5 {
					regulator-name = "vdd_gps";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO6: ldo6 {
					regulator-name = "vd1_io";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO7: ldo7 {
					regulator-name = "vd1_core";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
				RT5028_ID_LDO8: ldo8 {
					regulator-name = "vd2_io";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <3600000>;
					regulator-always-on;
				};
			};
		};
	};

	smui2c@74500000 {
		status = "okay";
	};

	ehci_phy@71EA0010{
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
	};

	dwc_otg_phy@71EA0100 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 3 0>;
	};

	ehci@71200000 {
		status = "okay";
		hcd_id = <0>;
		phy-type = <1>;	    //pico phy = 1, nano phy = 0
		//hosten-ctrl-able;
		//hosten-gpio = <&gp? ?? 0>;
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
		//vbus-source-ctrl;
		//pinctrl-names = "default";
		//vdd_ehci-supply = <&vdd_v5p0_reg>;

		//sq_level_ctl;
		TXVRT = <0xB>;
		TXRISET = <0x3>;
		TXAT = <0x1>;
	};

	ohci@71300000 {
		status = "okay";
		//hosten-ctrl-able;
		//hosten-gpio = <&gp? ?? 0>;
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 2 0>;
		//vbus-source-ctrl;
		//vdd_ohci-supply = <&vdd_v5p0_reg>;
	};

	dwc_otg@71B00000 {
		status = "okay";
		phy-type = <1>;     //pico phy = 1, nano phy = 0
		vbus-ctrl-able;
		vbus-gpio = <&gphdmi 3 0>;
	};

	/* SD CARD */
	vqmmc_sd: vqmmc_sd {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_sd";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	sdhc@76020000 {
		status = "okay";
		compatible = "telechips,tcc897x-sdhci";
		controller-id = <0>;
		bus-width = <4>;
		max-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd5_clk &sd5_cmd &sd5_bus4>;

		broken-cd;
		cd-inverted;
		keep-power-in-suspend;

		wp-gpios = <&gpf 20 0>;
		cd-gpios = <&gpf 21 0>;

		vqmmc-supply = <&vqmmc_sd>;
	};

	sdhc@76020400 { // Wi-Fi tcm3840
		status = "okay";
		controller-id = <2>;
		max-frequency = <208000000>;
		//cd-gpios = <&gpb 13 0>;
		//wp-gpios = <&gpext1 3 0>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd0_clk_idle &sd0_cmd_idle &sd0_bus4_idle &out0_clk>;
		pinctrl-1 = <&sd0_clk &sd0_cmd &sd0_bus4 &out0_clk>;
		clocks = <&clk_peri PERI_SDMMC2 &clk_io IOBUS_SDMMC2 &clk_peri PERI_OUT0>;
		clock-names = "mmc_fclk", "mmc_hclk", "ext32";
		//pwr-gpio = <&gpext2 10 0>;

		cd-inverted;
		//wp-inverted;
		//non-removable;
		//broken-cd;
		keep-power-in-suspend;
		//enable-sdio-wakeup;
		//cap-power-off-card;
		//tcc-clock-control;
		cap-sdio-irq;
		cap2-no-sleep-cmd;
		cap-bus-width-test;

		wifi-polling; // tcm3800 polling
		ext32-enable; // make_rtc
		cap-adma;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* High-speed DDR(emmc & SD3.0 DDR50) */
		//cap-1-8V-ddr;
		//cap-uhs-ddr50;

		/* SD3.0 Common */
		cap-vdd-165-195;

		/* SD3.0 SDR12 */
		//cap-uhs-sdr12;

		/* SD3.0 SDR25 */
		//cap-uhs-sdr25;
		//cap-uhs-driver_type_a;

		/* SD3.0 SDR50 */
		//cap-uhs-sdr50;
		//cap-uhs-driver_type_c;

		/* SD3.0 SDR104 */
		cap-uhs-sdr104;
		cap-uhs-driver_type_d;
		cap-uhs-cmd23;
		cap-adma;
	};

	/* eMMC */
	vqmmc_emmc: vqmmc_emmc {
		compatible = "regulator-fixed";
		regulator-name = "vqmmc_emmc";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	sdhc@76020600 {
		status = "okay";
		compatible = "telechips,tcc897x-sdhci";
		controller-id = <3>;
		bus-width = <8>;
		max-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd3_clk &sd3_cmd &sd3_bus8>;

		non-removable;
		keep-power-in-suspend;
		disable-wp;
		mmc-ddr-3_3v;

		vqmmc-supply = <&vqmmc_emmc>;
	};

	tcm3800 {
		compatible = "telechips, tcm3800";
		wlreg_on-gpio = <&gpsd 10 0>; /* WIFI_REG_ON */
		wifi_pwr-gpio = <&gpc 21 0>; /* BTWIFI_VBAT_EN */
	};

	tcc_bluetooth {
		compatible = "telechips, tcc_bluetooth";
		bt_power-gpio = <&gpc 21 0>;
		bt_reg_on-gpio = <&gpf 31 0>;
		status = "okay";
	};

	tcc_dxb_drv {
		compatible = "telechips,tcc_dxb_drv";
	};

	tcc_dxb_ctrl {
		compatible = "telechips,tcc_dxb_ctrl";
		/* DXB_ON */
		pw-gpios   = <&gpe 8 0>;
		/* PD RST IRQ */
		dxb0-gpios = <&gpe 27 0>, <&gpe 9 0>, <0>, <0>;
		dxb1-gpios = <&gpe 7 0>, <&gpe 6 0>, <0>, <0>;
		ant-gpios  = <0>, <0>;
	};

	i2s@76b01000 {
		compatible = "telechips,sdr";
		dev-name = "tcc-sdr";
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&s2dai_clks &s2dai_d0 &s2dai_d1 &s2dai_d2 &s2dai_d3>;
		pinctrl-1 = <&mute_s2dai_d0 &mute_s2dai_d1 &mute_s2dai_d2 &mute_s2dai_d3>;
		status = "okay";
	};

	pinctrl@74200000 {
		/*gpg-6 is for SDR_GPIO1*/
		mdai_clks: mdai_clks{
			telechips,pins = "gpg-7", "gpg-8";
			telechips,pin-function = <1>;
		};
	};

	i2s@76201000 { /*This is for BT hw:0,2*/
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&mdai_clks &mdai_d0>;
		pinctrl-1 = <&mute_mdai_d0>;
		clock-frequency = <48000>;
		status = "okay";
	};

	i2s@76401000 { /*This is for MainAudio hw:0,0*/
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&s1dai_clks &s1dai_d0>;
		pinctrl-1 = <&mute_s1dai_d0>;
		clock-frequency = <48000>;
		status = "okay";
	};

/* I2S2 connect to SDR
	i2s@76b01000 {
		pinctrl-names = "default";
		pinctrl-0 = <&s2dai_clks &s2dai_d0>;
		clock-frequency = <12288000>;	// 48000*256
		status = "okay";
	};
*/

	i2s@76101000 { /*This is for hw:0,5*/
		pinctrl-names = "default", "dai_mute";
		pinctrl-0 = <&s3dai_clks &s3dai_d0>;
		pinctrl-1 = <&mute_s3dai_d0>;
		clock-frequency = <48000>;
		status = "okay";
	};

	spdif@76202000 { /*This is for SPDIF TX1 hw:0,1*/
		pinctrl-names = "default";
		pinctrl-0 = <&mdai_spdif_tx &mdai_spdif_rx>;
		status = "okay";
	};

	spdif@76102000 { /*This is for SPDIF TX2 hw:0,3*/
		pinctrl-names = "default";
		pinctrl-0 = <&s3dai_spdif_tx>;
		status = "okay";
	};

	cdif@76101080 {	/*This is for SDR hw:0,4*/
		pinctrl-names = "default";
		pinctrl-0 = <&scdif2_clks &scdif2_d0>;
		status = "okay";
	};

	dummy: dummy@0 {
		compatible = "telechips,dummy";
	};

	sound {
		compatible = "telechips,snd-lcn3";
		telechips,model = "TCC8971 LCN2 Card";
		telechips,dai-controller = <&i2s1 &spdif0 &i2s0 &spdif1 &cdif1 &i2s3>;
		telechips,audio-codec = <&ak4601 &dummy &dummy &dummy &dummy &dummy>;
		status="okay";
	};

	tcc_switchmanager: tcc_switchmanager {
		pinctrl-names = "default";
		pinctrl-0 = <&switch_reverse2>;
		switch-gpios = <&gpd 4 0>;
		switch-active = <1>;
		status = "okay";
	};

	tcc_camera: tcc_camera {
		compatible = "telechips-camera";
		status = "okay";

		clocks = <&clk_peri PERI_OUT3 &clk_ddi DDIBUS_VIOC>;

		rear_cam_misc = <&rear_ramera>;

		camera_pgl	= <&vioc_rdma	 16>;
		camera_videoin	= <&vioc_vin	4 0>;
		camera_viqe	= <&vioc_viqe	  0>;
		camera_viqe_set	= <&viqe_hidden	   >;
		camera_deintls	= <&vioc_deintls  0>;
		camera_scaler	= <&vioc_scaler	  2>;
		camera_wmixer	= <&vioc_wmixer	  5>;
		camera_wdma	= <&vioc_wdma	  5>;
		camera_fifo	= <&vioc_fifo	  0>;
		camera_config	= <&vioc_config	  0>;

		pinctrl-names = "idle", "active";
		pinctrl-0 = <&cam4_idle>;
		pinctrl-1 = <&cam4_clki &cam4_hsync &cam4_vsync &cam4_data &cam4_fld>;
		//pinctrl-0 = <&cam1_idle>;
		//pinctrl-1 = <&cam1_clki &cam1_hsync &cam1_vsync &cam1_data &cam1_fld &cam1_de>;

		// cif port based on video source
		adv7182_atv {
			rst-gpios = <&gpe  4 1>;
			camera_port = <&cifport 4>;
		};
		dm5886_atv {
			rst-gpios = <&gpf 11 1>;
			camera_port = <&cifport 1>;
		};
	};

	overlaymixer:overlaymixer@72400000 {
		//status ="okay";
		contiguousSize = <0x1000000>;  /* 16MB (if > 512MB RAM) refer to Guide 27 Page */
		baseAddress = <0x80000000>;
		physSize = <0x80000000>; /*  DRAM size (0x40000000 = 1GB, 0x80000000 = 2GB) */
	};

	fbdisplay: fbdisplay@72000000 {
		telechips,fbdisplay_num = <0>;
		//Linuxplatform
		telechips,overlay_priority = <24>;
		//
		status ="okay";
	};

	tcc_vsync_drv {
		compatible = "telechips,tcc_vsync";
		display-vioc0 = <&vioc_display0>;
		display-vioc1 = <&vioc_display1>;
		status = "okay";
	};

	tcc_video_viqe {
		status ="okay";

		tcc_video_viqe_lcd {
			telechips,wmixer = <&vioc_wmixer 0>;
			telechips,rdma,60 = <&vioc_rdma 3>;
			telechips,disp = <&vioc_display0 0>;
			telechips,sc = <&vioc_scaler 1>;
			viqe_rdma_num_60 = <3>;
			scaler_num_60 = <1>;
			sc_rdma_num_60 = <3>;
			wmix_rdma = <3>;
		};

		tcc_video_viqe_external {
			telechips,wmixer = <&vioc_wmixer 1>;
			telechips,rdma,60 = <&vioc_rdma 7>;
			telechips,disp = <&vioc_display1 0>;
			telechips,sc = <&vioc_scaler 3>;
			viqe_rdma_num_60 = <5>;
			scaler_num_60 = <3>;
			sc_rdma_num_60 = <7>;
			wmix_rdma = <7>;
		};
	};

	vioc_lut:vioc_lut@72009000{
		status = "okay";
	};

	/* H/W Demux */
	hwdemux: cmbus@79000000 {
		compatible = "telechips,hwdemux";
		status = "okay";
	};

	gpsb1@76910000 {
		status = "okay";
		gpsb-port = <13>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb13_bus_idle>;
		pinctrl-1 = <&gpsb13_bus_tsif>;
	};

	gpsb2@76920000 {
		status = "okay";
		gpsb-port = <12>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb12_bus_idle>;
		pinctrl-1 = <&gpsb12_bus_tsif>;
	};

	/*tcc_ex_tsif {
		compatible = "telechips,tcc89xx-hwdmx-tsif";
		tsif-id = <2>;
		tsif-port = <6>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&ts6_idle>;
		pinctrl-1 = <&ts6_active>;
	};*/

	viocmg {
		compatible = "telechips,viocmg-drv";
		status = "okay";
		telechips,viocmg-fbdisplay = <&fbdisplay>;
		telechips,viocmg-camera = <&tcc_camera>;
		telechips,viocmg-rearcam = <&rear_ramera>;

		// RDMA in Display Path
		telechips,disp_rdma = <&vioc_rdma 2>;

		// feature
		telechips,rear_cam_enable = <1>;
		telechips,rear_cam_use_viqe = <1>;
		telechips,rear_cam_viqe_mode = <2>;
		telechips,rear_cam_use_parking_line = <1>;

		// rear cam mode
		telechips,rear_cam_mode = <0>;
		telechips,rear_cam_ovp = <13>;

		// hardware reg
		telechips,wmixer = <&vioc_wmixer>;
		telechips,config = <&vioc_config>;
	};

	rear_ramera:rear-camera {
		compatible = "telechips,rear-camera-drv";
		telechips,rear-camera = <&tcc_camera>;
		status = "okay";

		// preview
		telechips,preview_x = <0>;
		telechips,preview_y = <0>;
		telechips,preview_width = <1024>;
		telechips,preview_height = <600>;
		telechips,preview_format = <14>;
		telechips,preview_crop_x = <0>;
		telechips,preview_crop_y = <30>;
		telechips,preview_additional_width = <0>;
		telechips,preview_additional_height = <35>;

		// parking line
		telechips,parking_line_x = <0>;
		telechips,parking_line_y = <0>;
		telechips,parking_line_width = <1024>;
		telechips,parking_line_height = <600>;
		telechips,parking_line_format = <12>;
	};

	tcc_overlay_drv {
		compatible = "telechips,tcc_overlay";
		fbdisplay-overlay = <&fbdisplay>;
		clocks = <&clk_ddi DDIBUS_VIOC>;
		rdmas = <&vioc_rdma3 >, <&vioc_rdma7>;
		wmixs = <&vioc_wmix0 100 >, <&vioc_wmix1 100>;
		status = "okay";
	};

	wdma_drv0:wdma_drv@0 {
		status = "okay";
		clocks = <&clk_fbus FBUS_DDI>;
		wdma-fbdisplay = <&fbdisplay>;
		scalers = <&vioc_sc0 20>, <&vioc_sc0 21>;	/* id:1, path:20 (wdma 0 path), 21(wdma 1 path) */
		wdmas = <&vioc_wdma0>, <&vioc_wdma1>;	/* id:1, path: wmix output mixed pah */
		wmixs = <&vioc_wmix0 0 >, <&vioc_wmix1 2>;	/* id:1, path: input rdma mixed path */
	};

	vout_drv@0 {
		status = "okay";
	};

	scaler_drv@1 {
		rdmas = <&vioc_rdma16>;
		scalers = <&vioc_sc1 17>;	/* id:1, path:17 */
		wmixs = <&vioc_wmix5 6>;	/* id:5, path:6 */
		wdmas = <&vioc_wdma6>;
		settop_support = <0>;
		status = "okay";
	};

	scaler_drv@3 {
		rdmas = <&vioc_rdma7>;
		scalers = <&vioc_sc3 7>;	/* id:3, path:7 */
		wmixs = <&vioc_wmix1 2>;	/* id:1, path:2 */
		wdmas = <&vioc_wdma1>;
		settop_support = <0>;
		status = "okay";
	};

	wmixer_drv@0 {
		rdmas = <&vioc_rdma15>;
		scalers = <&vioc_sc0 15>;	/* id:0, path:15 */
		wmixs = <&vioc_wmix4 100>;	/* id:5, path:- */
		wdmas = <&vioc_wdma4>;
		status = "okay";
	};

	wmixer_drv@1 {
		rdmas = <&vioc_rdma13>;
		wmixs = <&vioc_wmix3 100>;	/* id:3, path:- */
		wdmas = <&vioc_wdma3>;
		status = "okay";
	};

