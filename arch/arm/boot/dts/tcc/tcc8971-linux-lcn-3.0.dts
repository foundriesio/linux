	gpio_keys_polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		autorepeat;

		button@0 {
			label = "Door Key";
			linux,code = <105>;
			gpios = <&gpa 0 0>;
		};

		button@1 {
			label = "Trunk Key";
			linux,code = <106>;
			gpios = <&gpa 3 0>;
		};
	};
};

/* console */
&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart22_data>;
	port-mux = <22>;
	status = "okay";
};

/* bluetooth */
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart30_data &uart30_rtscts>;
	port-mux = <30>;
	//dmas = <&dma0 0 29 &dma0 1 30>;
	//dma-names = "tx", "rx";
	status = "okay";
};

/* GPS */
&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart10_data>;
	port-mux = <10>;
	status = "okay";
};

