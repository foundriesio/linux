#include "tcc8031.dtsi"
#include "tcc803x-linux.dtsi"

/ {
	model = "Telechips TCC8031 Evaluation Board for AVN";
	compatible = "telechips,tcc8031", "telechips,tcc803x";

	chosen {
		bootargs = "vmalloc=480M console=ttyAMA0,115200n8";
		stdout-path = &uart0;
	};

	aliases {
	};

	pcie@11800000 {
		reset-gpio = <&gpsd0 11 0>;
		//status = "okay";
	};
/*
	nand@16400000 {
		compatible = "telechips,nand-v8";
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&nand_rdy &nand_cs0
			     &nand_we &nand_oe &nand_cle &nand_ale
			     &nand_wp &nand_databus8>;
	};
*/

	/* USB 3.0 DRD */
	dwc3_phy@11D90000 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpa 29 0>;
	};

	dwc3_platform {
		status = "okay"; 
		//usb3en-ctrl-able;                
		//usb3en-gpio = <&gpc 20 0>;       
		//vbus-source-ctrl;                
		vbus-ctrl-able;                    
		vbus-gpio = <&gpa 29 0>;           
		//pinctrl-names = "default";       
		//vdd_dwc-supply = <&vdd_v5p0_reg>;
	}; 

	/* USB 2.0 OTG */
	ehci_phy@11DA0010 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpa 28 0>;
	};

	ehci@11A00000 {
		status = "okay";
		hcd_id = <0>;
		phy-type = <1>;	    //pico phy = 1, nano phy = 0
		vbus-ctrl-able;
		vbus-gpio = <&gpa 28 0>;

		//sq_level_ctl;
		TXVRT = <0xB>;
		TXRISET = <0x3>;
		TXAT = <0x1>;
	};

	ohci@11A80000 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpa 28 0>;
	};

	dwc_otg_phy@11DA0100 {
		status = "okay";

		vbus-ctrl-able;
		vbus-gpio = <&gpa 27 0>;
	};

	mux_host_phy@11DA00DC {
		status = "okay";

		vbus-ctrl-able;
		vbus-gpio = <&gpa 27 0>;
	};

	dwc_otg@11980000 {
	    status = "okay";
	    phy-type = <1>;     //pico phy = 1, nano phy = 0

	    vbus-ctrl-able;
	    vbus-gpio = <&gpa 27 0>;
	};

	switch_reverse {
		compatible = "telechips,switch_reverse";
		status = "disabled";

		pinctrl-names = "default";
		pinctrl-0 = <&switch_reverse_ma15>;

		switch-gpios = <&gpma 15 1>;
		switch-active = <0>;
	};

	fbdisplay: fbdisplay@12000000 {
		telechips,fbdisplay_num = <0>;
		//Linuxplatform
		telechips,overlay_priority = <24>;
		//
		status ="okay";
	};

	thermal@14700000 {
		compatible = "telechips,tcc-thermal";
	reg = <0x14700000 0x4 0x14700030 0x4 0x14200290 0x4 0x1420029C 0x4
		0x14200298 0x4
		0x1470000C 0x4
		0x14700010 0x4>;
		status = "okay";

		throttle_count = <5>;
		throttle_active_count = <0>;
		throttle_passive_count = <5>;
                interrupts = <GIC_SPI 40 0>;
		cal_type = "TYPE_ONE_POINT_TRIMMING";
		polling-delay-idle = <0>;
		polling-delay-passive = <0>;

                threshold_temp = <85>;

		throttle_table {
			throttle_tab_0 {
				temp = <75>;
				freq_max_cluster0 = <1000000>;
				freq_max_cluster1 = <900000>;
			};
			throttle_tab_1 {
				temp = <80>;
				freq_max_cluster0 = <900000>;
				freq_max_cluster1 = <810000>;
			};
			throttle_tab_2 {
				temp = <85>;
				freq_max_cluster0 = <800000>;
				freq_max_cluster1 = <720000>;
			};
			throttle_tab_3 {
				temp = <90>;
				freq_max_cluster0 = <700000>;
				freq_max_cluster1 = <630000>;
			};
			throttle_tab_4 {
				temp = <95>;
				freq_max_cluster0 = <600000>;
				freq_max_cluster1 = <540000>;
			};
		};
	};

	cpu-id@14200290{
		compatible = "telechips,tcc-cpu-id";
		reg = <0x14200290 0x4 0x14200298 0x4 0x1420029C 0x4 0x14400014 0x4>;
		status = "okay";
	};

	overlaymixer:overlaymixer@1A100000 {
		status ="okay";
		contiguousSize = <0x1000000>;  /* 16MB (if > 512MB RAM) refer to Guide 27 Page */
		baseAddress = <0x20000000>;
		physSize = <0x80000000>; /*  DRAM size (0x40000000 = 1GB, 0x80000000 = 2GB) */
		};

	hdmi_tx_20@12400000 {
		hdmi_ref_src_clock = <0>; // 0 XIN, 1 X-TAL, 2 PERI, 3, PMU XIN
		hdmi_phy_type = <0>; // 0 8031

		/* Source Product Description */
		product_description = "TCC8031_avn";
	};


	i2c@16300000 {
		status = "okay";
		port-mux = <11>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c11_bus>;
		clock-frequency = <100000>;
	};

	i2c@16310000 {
		status = "okay";
		port-mux = <6>; /* [0]SCL [1]SDA */
		pinctrl-names = "default";
		pinctrl-0 = <&i2c6_bus>;

/* pmic da9062 */
		pmic@58 {
			compatible = "dlg,da9062";
			reg = <0x58>;
			int-gpios = <&gpma 12 0>;

			da9062_gpio: gpio {
				compatible = "dlg,da9062-gpio";
				gpio-controller;
				#gpio-cells = <2>;
			};

			regulators {
				DA9062_BUCK1: buck1 {
				regulator-name = "vdd_cpu0";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1570000>;
				regulator-min-microamp = <500000>;
				regulator-max-microamp = <2000000>;
				regulator-always-on;
				};

				DA9062_BUCK2: buck2 {
				regulator-name = "vdd_gv";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1570000>;
				regulator-min-microamp = <500000>;
				regulator-max-microamp = <2000000>;
				regulator-always-on;
				};

				DA9062_BUCK3: buck3 {
				regulator-name = "vdd_core";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3340000>;
				regulator-min-microamp = <500000>;
				regulator-max-microamp = <2000000>;
				regulator-always-on;
				};

				DA9062_BUCK4: buck4 {
				regulator-name = "vdd_cpu1";
				regulator-min-microvolt = <530000>;
				regulator-max-microvolt = <1800000>;
				regulator-min-microamp = <500000>;
				regulator-max-microamp = <2000000>;
				regulator-always-on;
				};

				DA9062_LDO1: ldo1 {
				regulator-name = "vdd_io";
				regulator-min-microvolt = <900000>;
				regulator-max-microvolt = <3600000>;
				regulator-always-on;
				};
#if 0
			 /* Temp block for LDO2. Block min-microvolt of LDO2 */
				DA9062_LDO2: ldo2 {
				regulator-name = "mem_1p8";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
				};
#endif
				DA9062_LDO4: ldo4 {
					regulator-name = "sdio_pwr";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3300000>;
					regulator-always-on;
					regulator-boot-on;
				};
			};
		}; /* pmic da9062 */

		mxt_touch@4a {
			compatible = "atmel,maxtouch";
			reg = <0x4a>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;

			irq-gpios = <&gpsd1 9 0x0>;
			reset-gpios = <&gpb 16 0x0>;
		};
	};


	i2c@16320000 {
		status = "okay";
		port-mux = <13>; /* [0]SCL [1]SDA */
		pinctrl-names = "default";
		pinctrl-0 = <&i2c13_bus>;
		ak4601: ak4601@10 {
			compatible = "akm,ak4601";
			ak4601,pdn-gpio = <&gpb 27 0>;
			cmute-gpios = <&gpb 28 1>;	//CODEC_MUTE    0: active high, 1: active low
			amute-gpios = <&gpma 27 0>;	//AMP_MUTE      0: active high, 1: active low
//			amute-gpios = <&gpmb 27 0>;	//AMP_MUTE      0: active high, 1: active low
			stanby-gpios = <&gpma 26 0>;	//AMP_STBY      0: active high, 1: active low
//			stanby-gpios = <&gpmb 26 0>;	//AMP_STBY      0: active high, 1: active low
			reg = <0x10>;
		};
	};

	i2c@16330000 {
		status = "okay";
		port-mux = <22>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c22_bus>;
		clock-frequency = <400000>;
	};

	i2c@16370000 {
		status = "okay";
		port-mux = <12>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c12_bus>;
		clock-frequency = <400000>;
	};

	gpsb0@16900000 {
		status = "okay";  // enable = "okay"
		gpsb-port = <22>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb22_bus_idle>;
		pinctrl-1 = <&gpsb22_bus_spi>;

		/* cs-gpios */
		cs-gpios = <&gpg 8 0>,<&gpa 30 0>;

		#address-cells = <1>;
		#size-cells = <0>;
		spidev@0 {
			compatible = "rohm,dh2228fv";
			reg = <0>;
			spi-max-frequency = <2000000>;
		};
		spidev@1 {
			compatible = "rohm,dh2228fv";
			reg = <1>;
			spi-max-frequency = <2000000>;
		};
	};

	gpsb1@16910000 {
		status = "okay";
		gpsb-port = <15>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb15_bus_idle>;
		pinctrl-1 = <&gpsb15_bus_tsif>;
	};

	gpsb2@16920000 {
		status = "okay";
		gpsb-port = <16>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb16_bus_idle>;
		pinctrl-1 = <&gpsb16_bus_tsif>;
	};

	//This is for SDR solution.
	pinctrl@14200000 {
		m1dai1_bclk: m1dai1_bclk {
			 telechips,schmitt-input;
		};
		m1dai1_lrck: m1dai1_lrck {
			 telechips,schmitt-input;
		};
		m1dai1_dai0: m1dai1_dai0 {
			 telechips,schmitt-input;
		};
		m1dai1_dai1: m1dai1_dai1 {
			 telechips,schmitt-input;
		};
		m1dai1_dai2: m1dai1_dai2 {
			 telechips,schmitt-input;
		};
		m1dai1_dai3: m1dai1_dai3 {
			 telechips,schmitt-input;
		};
		m1dai0_bclk: m1dai0_bclk {
			 telechips,schmitt-input;
		};
		m1dai0_lrck: m1dai0_lrck {
			 telechips,schmitt-input;
		};
		m1dai0_dai0: m1dai0_dai0 {
			 telechips,schmitt-input;
		};
		m1dai0_dai1: m1dai0_dai1 {
			 telechips,schmitt-input;
		};
		m1dai0_dai2: m1dai0_dai2 {
			 telechips,schmitt-input;
		};
		m1dai0_dai3: m1dai0_dai3 {
			 telechips,schmitt-input;
		};
	};

	/* sdr */
	i2s@16801000 {
		compatible = "telechips,sdr";
		dev-name = "tcc-iq01";
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&m1dai1_bclk &m1dai1_lrck &m1dai1_dai0 &m1dai1_dai1 &m1dai1_dai2 &m1dai1_dai3>;
		pinctrl-1 = <&m1dai1_bclk_idle &m1dai1_lrck_idle &m1dai1_dai0_idle &m1dai1_dai1_idle &m1dai1_dai2_idle &m1dai1_dai3_idle>;
		status = "okay";
	};	

	i2s@16701000 {
		compatible = "telechips,sdr";
		dev-name = "tcc-iq23";
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&m1dai0_bclk &m1dai0_lrck &m1dai0_dai0 &m1dai0_dai1>;
		pinctrl-1 = <&m1dai0_bclk_idle &m1dai0_lrck_idle &m1dai0_dai0_idle &m1dai0_dai1_idle>;
		status = "okay";
	};


	i2s@16A01000 {
		compatible = "telechips,sdr";
		dev-name = "tcc-hdr-blend";
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&m0dai1_bclk &m0dai1_lrck &m0dai1_dai0>;
		pinctrl-1 = <&m0dai1_bclk_idle &m0dai1_lrck_idle &m0dai1_dai0_idle>;
		status = "okay";
	};

	audio_chmux@16051000{
		compatible = "telechips,audio-chmux-803x";
		dai = <3 0 1 0 1 99 99>;	// i2s0(x) i2s1(x) i2s2(x) i2s3(x) i2s4(x) i2s5(x) i2s6(x)
		spdif = <2 99 99 3 99 99 99>;	// spdif0(x) spdif1(x) spdif2(x) spdif3(x) spdif4(x) spdif5(x) spdif6(x)
		status = "okay";
	};

	adma@16100000 {
		status = "okay";
	};

	adma@16700000 {
		status = "disabled";
	};

	adma@16800000 {
		status = "disabled";
	};
	
	adma@16200000 {
		status = "okay";
	};

	adma@16A00000 {
		status = "disabled";
	};
	
	i2s@16101000 {
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&m1dai3_mclk &m1dai3_bclk &m1dai3_lrck &m1dai3_dao0 &m1dai3_dai0>;
		pinctrl-1 = <&m1dai3_mclk_idle &m1dai3_bclk_idle &m1dai3_lrck_idle &m1dai3_dao0_idle &m1dai3_dai0_idle>;
		status = "okay";		

	};

	i2s@16201000 {
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&m0dai0_bclk &m0dai0_lrck &m0dai0_dao0 &m0dai0_dai0>;
		pinctrl-1 = <&m0dai0_bclk_idle &m0dai0_lrck_idle &m0dai0_dao0_idle &m0dai0_dai0_idle>;
		status = "okay";		

	};

	
	spdif@16102000 {
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&spdif2_tx>;
		pinctrl-1 = <&spdif2_tx_idle>;
		status = "okay";
	};

	spdif@16202000 {
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&spdif3_rx>;
		pinctrl-1 = <&spdif3_rx_idle>;
		status = "okay";
	};

