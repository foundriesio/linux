/dts-v1/;

#include "tcc8995-android-mini.dtsi"

/ {
	/* Describe here after remove this statement if you have something to be override. */
	gpio_keys_polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		autorepeat;
		button@0 {
			label = "BT Auto Pair Key";
			linux,code = <218>;
			gpios = <&gpf 30 1>;
		};
	};

	cpu0-regulators {
		compatible = "regulator-gpio";

		regulator-name = "vdd_cpu0";
		regulator-min-microvolt = <700000>;
		regulator-max-microvolt = <900000>;
		regulator-type = "voltage";
		regulator-always-on;

		gpios = <&gpd 16 0x4
		         &gpd 17 0x4>;

		gpios-states = <1 0>;

		states = < 700000 0x0
		           700000 0x1
		           800000 0x2
		           900000 0x3
		           >;

		startup-delay-us = <100000>;
		enable-active-high;
	};
};
