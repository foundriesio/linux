	/* for EVT0/EVT1 */
	pcie@11500000 {
		compatible = "telechips,pcie", "snps,dw-pcie";
		reg = <0x11500000 0x1000
		       0x11600000 0x1000
		       0x11610000 0x1000
		       0x114e0000 0x20000>;
		reg-names = "dbi", "phy", "cfg", "config";
		interrupts = <GIC_SPI 100 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&clk_peri PERI_PCIE_AUX>, <&clk_peri PERI_PCIE_APB>, <&clk_peri PERI_PCIE_REF_EXT>,
			 <&clk_fbus FBUS_HSIO>, <&clk_isoip_top ISOIP_TOP_PCIE>;
		clock-names = "pcie_aux", "pcie_apb", "pcie_ref_ext", "fbus_hsio", "pcie_phy";
		#address-cells = <3>;
		#size-cells = <2>;
		device_type = "pci";
		ranges = <0x81000000 0 0          0x114d0000 0 0x00010000   /* downstream I/O */
			  0x82000000 0 0x11400000 0x11400000 0 0x000d0000>; /* non-prefetchable memory */
		#interrupt-cells = <1>;
		interrupt-map-mask = <0 0 0 0>;
		interrupt-map = <0 0 0 0 &gic GIC_SPI 100 IRQ_TYPE_LEVEL_HIGH>;
		num-lanes = <1>;

		/* PHY setting */
		dcc_feedback = <0>;
		tx_impedance = <0x5>;
		rx_impedance = <0xd>;
		tx_amplitude = <0x35>;
		tx_deemphasis = <0x14>;
		pll_bias = <0x7>;
		rx_cdr_bw = <0x82>;

		status = "disabled";
	};

