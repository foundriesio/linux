	hdmi_tx_20: hdmi_tx_20@12400000 {
		compatible = "telechips,dw-hdmi-tx";
		/* Register */
		reg = <0x12400000 0x20000 0x12440000 0x1000 0x12480000 0x1000 0x163C0000 0x10 0x16051000 0x000018>;
		reg-names = "hdmi_link", "hdcp_2.2", "hdmi_phy", "i2c_port_cfg";
		/* Interrupt */

		interrupts = <GIC_SPI 85 IRQ_TYPE_LEVEL_HIGH>,
				<GIC_SPI 86 IRQ_TYPE_LEVEL_HIGH>, 
				<GIC_SPI 87 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "hdmi_link", "hdcp_2.2", "hdmi_cec";
		/* Clocks */
		clocks = <&clk_peri PERI_HDMI_PHY27M>, 
				<&clk_peri PERI_HDMI_SPDIFCLK>, 
				<&clk_peri PERI_HDMI_PCLK>, 
				<&clk_peri PERI_HDMI_CEC>, 
				<&clk_peri PERI_HDMI_HDCP14>, 
				<&clk_peri PERI_HDMI_HDCP22>, 
				<&clk_ddi DDIBUS_HDMI>, 
				<&clk_isoip_ddi ISOIP_DDB_HDMI>;
		clock-names = "hdmi_ref_clk", 
				"hdmi_spdif_clk", 
				"hdmi_apb_clk", 
				"hdmi_cec", 
				"hdmi_hdcp14", 
				"hdmi_hdcp22", 
				"hdmi_ddibus", 
				"hdmi_isoip_ddibus";
		clock-frequency = <50000000>;
		/* Pinctrl and gpio */
		pinctrl-names = "default";
		pinctrl-0 = <&hdmi_hpd &hdmi_i2c>;
		gpios = <&gphdmi 1 0>;
		/* DDC */
		hdmi_i2c_port_mapping = <0xc 0xf 0xe>; /* offset mask value */
		audio_if_selection_ofst = <0x58>;
		verbose = <0>; // 0 disable, 1 basic, 2 io
		status = "disabled";
	};

