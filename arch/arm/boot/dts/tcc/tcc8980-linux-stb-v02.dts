/dts-v1/;

#include "tcc8980.dtsi"
#include "tcc898x-linux.dtsi"

/ {
	model = "Telechips TCC8980 STB Board";
	compatible = "telechips,tcc8980", "telechips,tcc898x";

	chosen {
	};

	memory@20000000 {
		device_type = "memory";
		reg = <0x20000000 0x80000000>;
	};

	cpu0-regulators {
		compatible = "regulator-gpio";

		regulator-name = "vdd_cpu0";
		regulator-min-microvolt = < 950000>;
		regulator-max-microvolt = <1200000>;
		regulator-type = "voltage";
		regulator-always-on;

		gpios = <&gpd 25 0x4
			 &gpd 24 0x4>;
		gpios-states = <0 1>;

		states = < 950000 0x0
			  1000000 0x1
			  1100000 0x2
			  1200000 0x3
		>;

		startup-delay-us = <100000>;
		enable-active-high;
	};

	panel {
		status = "okay";
		compatible = "telechips,hdmi4k";
	};

	tve@12200000 {
		status = "okay";
	};

	composite {
		status = "okay";
	};

	component {
		status = "okay";
		io_port_num = <0>;
	};

	output_starter {
		status = "okay";
		//persist_display_mode = <3>;
	};

	overlaymixer:overlaymixer@1A100000 {
		status ="okay";
		contiguousSize = <0x1000000>;  /* 16MB (if > 512MB RAM) refer to Guide 27 Page */
		baseAddress = <0x20000000>;
		physSize = <0x80000000>; /*  DRAM size (0x40000000 = 1GB, 0x80000000 = 2GB) */
	};

	fbdisplay: fbdisplay@12000000 {
		telechips,fbdisplay_num = <0>;
		status ="okay";
	};

	tcc_overlay_drv {
		compatible = "telechips,tcc_overlay";
		status = "okay";
		fbdisplay-overlay = <&fbdisplay>;
	};

	tcc_vsync_drv {
		compatible = "telechips,tcc_vsync";
		display-vioc0 = <&vioc_display0>;
		display-vioc1 = <&vioc_display1>;
		status = "okay";
	};

	tcc_video_viqe {
		compatible = "telechips,tcc_viqe";
		status ="okay";
		tcc_viqe_viqe_common {
			board_num=<1>;
		};
	};

	scaler_drv@1 {
		rdmas = <&vioc_rdma16>;
		scalers = <&vioc_sc1 17>;	/* id:1, path:17 */
		wmixs = <&vioc_wmix5 6>;	/* id:5, path:6 */
		wdmas = <&vioc_wdma6>;
		settop_support = <1>;
		status = "okay";
	};

	scaler_drv@3 {
		rdmas = <&vioc_rdma7>;
		scalers = <&vioc_sc3 7>;	/* id:3, path:7 */
		wmixs = <&vioc_wmix1 2>;	/* id:1, path:2 */
		wdmas = <&vioc_wdma1>;
		settop_support = <1>;
		status = "okay";
	};

	wmixer_drv@0 {
		status = "okay";
	};

	wmixer_drv@1 {
		status = "okay";
	};

	vout_drv@0 {
		status = "okay";
	};

	sdhc@16440000 { // Wi-Fi tcm3830
		status = "okay";
		controller-id = <0>;
		bus-width = <4>;
		max-frequency = <200000000>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd0_1_4_idle>;
		pinctrl-1 = <&sd0_1_clk &sd0_1_cmd &sd0_1_bus4>;
		cd-inverted;
		//wp-inverted;
		//non-removable;
		//broken-cd;
		keep-power-in-suspend;
		//enable-sdio-wakeup;
		//cap-power-off-card;
		cap-sdio-irq;
		cap2-no-sleep-cmd;
		cap-bus-width-test;

		//wp-gpios = <&gpb 7 0>;
		//cd-gpios = <&gpext1 5 0>;
		//vctrl-gpio = <&gpsd 10 0>;
		//pwr-gpio = <&gpext2 7 0>;

		//cd-polling;
		wifi-polling;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* High-speed DDR(emmc & SD3.0 DDR50) */
		//cap-1-8V-ddr;
		//cap-uhs-ddr50;

		/* emmc hs200 & SD3.0 Common */
		cap-vdd-165-195;

		/* SD3.0 SDR12 */
		//cap-uhs-sdr12;

		/* SD3.0 SDR25 */
		//cap-uhs-sdr25;
		//cap-uhs-driver_type_a;

		/* SD3.0 SDR50 */
		//cap-uhs-sdr50;
		//cap-uhs-driver_type_c;

		/* SD3.0 SDR104 */
		cap-uhs-sdr104;
		cap-uhs-driver_type_d;
		cap-uhs-cmd23;
		cap-adma;

		/* eMMC HS200 */
		//mmc-hs200-1_8v;
		//cap-uhs-driver_type_d;
		//cap-uhs-cmd23;
		//cap-adma;
	};

	tcm3800 {
		compatible = "telechips, tcm3800";
		wlreg_on-gpio = <&gpg 18 0>;
	};

	tcc_bluetooth {
		compatible = "telechips, tcc_bluetooth";
		bt_wake-gpios = <&gpa 29 0>;
		bt_hwake-gpio = <&gpg 11 0>;
		bt_reg_on-gpio = <&gpa 30 0>;
		wifi_reg_on-gpio = <&gpg 18 0>;
		status = "okay";
	};

	tcc_bt_dev {
		compatible = "telechips, tcc_bt_dev";
		bt_reset-gpio = <&gpa 30 0>;
		status = "okay";
	};

	sdhc@16450000 { // eMMC Boot
		status = "okay";
		controller-id = <1>;
		bus-width = <8>;
		max-frequency = <200000000>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd1_1_idle>;
		pinctrl-1 = <&sd1_1_clk &sd1_1_cmd &sd1_1_bus8>;
		//cd-inverted;
		//wp-inverted;
		non-removable;
		//broken-cd;
		//keep-power-in-suspend;
		//enable-sdio-wakeup;
		//cap-power-off-card;
		cap-sdio-irq;
		cap2-no-sleep-cmd;
		cap-bus-width-test;

		//wp-gpios = <&gpb 7 0>;
		//cd-gpios = <&gpext1 5 0>;
		vctrl-gpio = <&gpa 5 0>;
		//pwr-gpio = <&gpext2 7 0>;
		emmc-rst-gpio = <&gpa 6 0>; // To do : add handling code

		cd-polling;
		//wifi-polling;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* High-speed DDR(emmc & SD3.0 DDR50) */
		cap-1-8V-ddr;
		cap-uhs-ddr50;

		/* emmc hs200 & SD3.0 Common */
		//cap-vdd-165-195;

		/* SD3.0 SDR12 */
		//cap-uhs-sdr12;

		/* SD3.0 SDR25 */
		//cap-uhs-sdr25;
		//cap-uhs-driver_type_a;

		/* SD3.0 SDR50 */
		//cap-uhs-sdr50;
		//cap-uhs-driver_type_c;

		/* SD3.0 SDR104 */
		//cap-uhs-sdr104;
		//cap-uhs-driver_type_d;
		//cap-uhs-cmd23;
		//cap-adma;

		/* eMMC HS200 */
		//mmc-hs200-1_8v;
		//cap-uhs-driver_type_d;
		//cap-uhs-cmd23;
		//cap-adma;
	};

	sdhc@16460000 { // SD Card
		status = "okay";
		controller-id = <2>;
		bus-width = <4>;
		max-frequency = <200000000>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd2_1_idle>;
		pinctrl-1 = <&sd2_1_clk &sd2_1_cmd &sd2_1_bus8>;
		cd-inverted;
		//wp-inverted;
		//non-removable;
		//broken-cd;
		//keep-power-in-suspend;
		//enable-sdio-wakeup;
		//cap-power-off-card;
		cap-sdio-irq;
		cap2-no-sleep-cmd;
		cap-bus-width-test;

		wp-gpios = <&gpsd0 10 0>;
		cd-gpios = <&gpc 25 0>;
		vctrl-gpio = <&gpd 22 0>;
		//pwr-gpio = <&gpext2 7 0>;

		//cd-polling;
		//wifi-polling;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* High-speed DDR(emmc & SD3.0 DDR50) */
		//cap-1-8V-ddr;
		//cap-uhs-ddr50;

		/* emmc hs200 & SD3.0 Common */
		//cap-vdd-165-195;

		/* SD3.0 SDR12 */
		//cap-uhs-sdr12;

		/* SD3.0 SDR25 */
		//cap-uhs-sdr25;
		//cap-uhs-driver_type_a;

		/* SD3.0 SDR50 */
		//cap-uhs-sdr50;
		//cap-uhs-driver_type_c;

		/* SD3.0 SDR104 */
		//cap-uhs-sdr104;
		//cap-uhs-driver_type_d;
		//cap-uhs-cmd23;
		//cap-adma;

		/* eMMC HS200 */
		//mmc-hs200-1_8v;
		//cap-uhs-driver_type_d;
		//cap-uhs-cmd23;
		//cap-adma;
	};

	hdmi_tx_20@12400000 {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hdmi_hpd>;
		hdmi_i2c_mapping = <&i2c3>;
		hdmi_i2c_mapping_val = <0xC 0xF 0x0E>;
		verbose = <0>; // 0 disable, 1 basic, 2 io
		gpios = <&gphdmi 1 0>; // HPD for output starter
		hdmi_phy_type = <0>; // 0 8980, 1 8985_OTT
	};

	hdmi_cec_20@16053000 {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hdmi_cec>;
	};


	tcc_dxb_drv {
		compatible = "telechips,tcc_dxb_drv";
	};

	tcc_dxb_internal {
		compatible = "telechips,tcc_dxb_internal";
		hwdmx_num = <1>;
	};

	tcc_dxb_ctrl {
		compatible = "telechips,tcc_dxb_ctrl";
		pw-gpios   = <&gpe 20 0>;
		dxb0-gpios = <&gpe 22 0>, <&gpe 21>, <&gpd 2 0>, <>;
		dxb1-gpios = <>, <>, <>, <>;
		ant-gpios  = <>, <>;
	};

	/* I2C */

	// Default set
	i2c@16300000 {
		port-mux = <9>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&i2c9_bus>;

		hdmi_in@78 {
			compatible = "tcc-hdin-i2c";
			reg = <0x3C>;
		};

		vtl_ct26x@01 {
			compatible = "vtl,ct36x";
			reg = <0x01>;
			pinctrl-names = "default";
			pinctrl-0 = <&tsc_default>;
			int-gpios = <&gpd 7 0>;
			rst-gpios = <&gpd 10 0>;
			interrupts = <GIC_SPI 0 IRQ_TYPE_LEVEL_HIGH>;
		};

		adv7343@20 {
			compatible = "component-adv7343-i2c";
			reg = <0x2b>;	/* ALSB=1, Write=0x56, Read=0x57 */
		};
	};

	// DXB
	i2c@16310000 {
		port-mux = <8>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&i2c8_bus>;
	};

	/* Not use
	i2c@16320000 {
		port-mux = <18>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&i2c18_bus>;
	};
	*/

	// HDMI
	i2c@16330000 {
		port-mux = <14>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&i2c14_bus>;
	};

	hdmi_in {
		compatible = "telechips-hdin";
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hdin_clki &hdin_hsync &hdin_vsync &hdin_data &hdin_de>;
		clocks = <&clk_ddi DDIBUS_VIOC>;
		hdin_port = <&cifport 3 1>;
		hdin_wmixer = <&vioc_wmixer 5>;
		hdin_wdma = <&vioc_wdma 5>;
		hdin_videoin = <&vioc_vin 4 0>;
		hdin_scaler = <&vioc_scaler 0>;
		hdin_config = <&vioc_config 0>;
		hdin_viqe = <&vioc_viqe 0>;

		ep9553t{
			rst-gpios = <&gpe 31 0>;
			pwr-gpios = <&gpf 30 0>;
			key-gpios = <&gpg 15 0>;
			int-gpios = <&gpg 13 1>;
			polarity_hsync = <1>;
			polarity_vsync = <1>;
			field_bfield_low = <0>;
			polarity_data_en = <0>;
			gen_field_en = <1>;
			polarity_pclk = <1>;
			conv_en = <0>;
			hsde_connect_en = <0>;
			vs_mask = <0>;
			input_fmt = <0>;
			data_order = <5>;
			intpl_en = <0>;
			wdma_swap_order = <0>;
		};
	};

	/* H/W Demux */
	hwdemux: cmbus@19000000 {
		compatible = "telechips,hwdemux";
		status = "okay";
	};

	/* GPSB */
	gpsb0@16900000 {
		status = "okay";
		gpsb-port = <3>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb3_bus_idle>;
		pinctrl-1 = <&gpsb3_bus_spi>;

		#address-cells = <1>;
		#size-cells = <0>;
		spidev@0 {
			compatible = "rohm,dh2228fv";
			reg = <0>;
			spi-max-frequency = <2000000>;
		};
	};

	gpsb1@16910000 {
		status = "okay";
		gpsb-port = <5>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb5_bus_idle>;
		pinctrl-1 = <&gpsb5_bus_tsif>;
	};

/*
	gpsb2@16920000 {
		status = "okay";
		gpsb-port = <4>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&gpsb4_bus_idle>;
		pinctrl-1 = <&gpsb4_bus_tsif>;
	};
*/

	tcc_ex_tsif {
		compatible = "telechips,tcc89xx-hwdmx-tsif";
		tsif-id = <2>;
		tsif-port = <2>;
		port-mux = <2>;
		pinctrl-names = "idle", "active";
		pinctrl-0 = <&tsrx2_idle>;
		pinctrl-1 = <&tsrx2_active>;
	};

/* TODO : Need to decide port for each gpsb channel
	gpsb3@16930000 {
		status = "okay";
		gpsb-port = <4>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpsb5_bus>;
	};

	gpsb4@16940000 {
		status = "okay";
		gpsb-port = <?>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpsb5_bus>;
	};

	gpsb5@16950000 {
		status = "okay";
		gpsb-port = <?>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpsb5_bus>;
	};
*/

	/* USB 3.0 DRD */
	dwc3@11900000 {
		status = "okay";
		//usb3en-ctrl-able;
		//usb3en-gpio = <&gpc 22 0>;
		//vbus-source-ctrl;
		vbus-ctrl-able;
		vbus-gpio = <&gpc 22 0>;
		//pinctrl-names = "default";
		//vdd_dwc-supply = <&vdd_v5p0_reg>;
	};

	/* USB 2.0 HOST */
	ehci_phy@111A0010 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpc 20 0>;
	};

	mux_host_phy@111A00DC {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpc 21 0>;
	};

	dwc_otg_phy@111A0100 {
		status = "okay";
		vbus-ctrl-able;
		vbus-gpio = <&gpc 21 0>;
	};

	ehci@11200000 {
		status = "okay";
		hcd_id = <0>;
		phy-type = <1>;	    //pico phy = 1, nano phy = 0
		hosten-ctrl-able;
		hosten-gpio = <&gpe 15 0>;
		vbus-ctrl-able;
		vbus-gpio = <&gpc 20 0>;
		//vbus-source-ctrl;
		//pinctrl-names = "default";
		//vdd_ehci-supply = <&vdd_v5p0_reg>;

		//sq_level_ctl;
		TXVRT = <0xB>;
		TXRISET = <0x3>;
		TXAT = <0x1>;
	};

	ohci@11300000 {
		status = "okay";
		hosten-ctrl-able;
		hosten-gpio = <&gpe 15 0>;
		vbus-ctrl-able;
		vbus-gpio = <&gpc 20 0>;
		//vbus-source-ctrl;
		//vdd_ohci-supply = <&vdd_v5p0_reg>;
	};

	/* USB 2.0 OTG */
	dwc_otg@11080000 {
	    status = "okay";
	    phy-type = <1>;     //pico phy = 1, nano phy = 0

	    vbus-ctrl-able;
	    vbus-gpio = <&gpc 21 0>;
	};

	tcc_cipher@111C0000 {
		status = "okay";
	};

	i2s@16101000 {
		pinctrl-names = "default";
		pinctrl-0 = <&m0dai1_clks &m0dai1_d0>;
		port-mux = <1>;
		status = "disable";
	};

	i2s@16201000 {
		pinctrl-names = "default";
		pinctrl-0 = <&m1dai1_clks &m1dai1_d0>;
		port-mux = <1>;
		status = "okay";
	};

	spdif@16102000 {
		pinctrl-names = "default";
		pinctrl-0 = <&m0dai1_spdif_tx>;
		port-mux = <1>;
		status = "disable";
	};

	spdif@16202000 {
		pinctrl-names = "default";
		pinctrl-0 = <&m1dai1_spdif_tx>;
		port-mux = <1>;
		status = "okay";
	};

	adma@16100000 {
		status = "disable";
	};

	adma@16200000 {
		status = "okay";
	};

	wm8524: wm8524@0 {
		compatible = "wolfson,wm8524";
		status = "okay";

		codec_aif-gpios = <&gpd 6 0>;
		codec_mute_ang-gpios = <&gpsd1 7 0>;
		codec_mute_ctl-gpios = <&gpc 30 0>;
		/* value to enable mute*/
		codec_mute_ang-mute_value = <0>;
	};

	sound@0 {
		compatible = "telechips,snd-card";
		card-name = "TCC8980 STB Card";
		status = "okay";

		telechips,dai-link@0 {
			stream-name="I2S_1";

			codec,format = "i2s"; // i2s, left_j, right_j
			codec,continuous-clock;

			mclk_div = <16>; // 4,6,8,16
			bclk_ratio = <64>; // 32,48,64fs
			pcm = <&adma1>;
			dai = <&i2s1>;
			codec = <&wm8524>;
			codec,dai-name = "wm8524-hifi";
		};

		telechips,dai-link@1 {
			stream-name="SPDIF_1";

			pcm = <&adma1>;
			dai = <&spdif1>;
		};
	};
	remote@16040000 {
		int-gpios = <&gpc 28 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&remote_c28>;
		wakeupsrc = <011>; //RMISEL is 0x3 : GPIO_C28
		status = "okay";
	};

	gmac@11700000 {
		status = "okay";
		phyrst-gpio = <&gpc 9 0>;
		phy-interface = "rgmii";
		ecid-mac-addr;
		txclk-o-dly = <0>;
		txclk-o-dly2 = <6>;
		txclk-o-inv = <0>;
		txclk-i-dly = <0>;
		txclk-i-inv = <0>;
		txen-dly = <0>;
		txer-dly = <0>;
		txd0-dly = <0>;
		txd1-dly = <0>;
		txd2-dly = <0>;
		txd3-dly = <0>;
		txd4-dly = <0>;
		txd5-dly = <0>;
		txd6-dly = <0>;
		txd7-dly = <0>;
		rxclk-i-dly = <0>;
		rxclk-i-inv = <0>;
		rxdv-dly = <0>;
		rxer-dly = <0>;
		rxd0-dly = <0>;
		rxd1-dly = <0>;
		rxd2-dly = <0>;
		rxd3-dly = <0>;
		rxd4-dly = <0>;
		rxd5-dly = <0>;
		rxd6-dly = <0>;
		rxd7-dly = <0>;
		crs-dly = <0>;
		col-dly = <0>;
		pinctrl-names = "default", "mii", "rmii", "gmii", "rgmii";
		pinctrl-0 = <>;
		pinctrl-1 = <&gmac1_mdc &gmac1_mdio &gmac1_col &gmac1_crs &gmac1_txer &gmac1_rxer
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3>;
		pinctrl-2 = <&gmac1_mdc &gmac1_mdio
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1
					 &gmac1_rxer &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1>;
		pinctrl-3 = <&gmac1_mdc &gmac1_mdio &gmac1_col &gmac1_crs &gmac1_txer &gmac1_rxer
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_txd4 &gmac1_txd5 &gmac1_txd6 &gmac1_txd7
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3
					 &gmac1_rxd4 &gmac1_rxd5 &gmac1_rxd6 &gmac1_rxd7>;
		pinctrl-4 = <&gmac1_mdc &gmac1_mdio
					 &gmac1_txclk &gmac1_txen &gmac1_txd0 &gmac1_txd1 &gmac1_txd2 &gmac1_txd3
					 &gmac1_rxclk &gmac1_rxdv &gmac1_rxd0 &gmac1_rxd1 &gmac1_rxd2 &gmac1_rxd3>;
	};

	/* evt0/evt1 */
	pcie@11500000 {
		reset-gpio = <&gpc 27 0>;
		status = "okay";
	};

	pcie@11680000 {
		reset-gpio = <&gpc 27 0>;
		status = "okay";
	};

	smartcard {
		compatible = "telechips,pl131-smartcard";
		reg = <0x16661000 0xFFF 0x16684000 0x10>;                //smartcard #6,
		clocks = <&clk_io IOBUS_SMARTCARD6>;
		clock-frequency = <50000000>;
		smartcard-clock = <4000000>;                             // initail value 4Mhz
		Fi = <372>;						 // clock rate conversion integer
		Di = <1>;						 // initail value

		uart_ch = <&uart6>;					 // uart and smartcard share clock
		uart_chnum = <6>;
		port-num = <0>;
		tda8024-use = <1>;
		rst_gpios =  <&gpb 31 0>;
		detect_gpios = <&gpb 28 0>;
		pwren_gpios = <&gpb 30 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&smartcard0_bus>;
		status = "okay";
	};
	thermal@14700000 {
		compatible = "telechips,tcc-thermal";
		reg = <0x14700000 0x4 0x14700030 0x4 0x14200290 0x4 0x1420029C 0x4>;
		clocks = <&clk_isoip_top ISOIP_TOP_TSEN>;
		status = "okay";

		throttle_count = <5>;
		throttle_active_count = <0>;
		throttle_passive_count = <5>;

		cal_type = "TYPE_ONE_POINT_TRIMMING";
		polling-delay-idle = <0>;
		polling-delay-passive = <0>;

		throttle_table {
			throttle_tab_0 {
				temp = <75>;
				freq_max_cluster0 = <1000000>;
				freq_max_cluster1 = <900000>;
			};
			throttle_tab_1 {
				temp = <80>;
				freq_max_cluster0 = <900000>;
				freq_max_cluster1 = <810000>;
			};
			throttle_tab_2 {
				temp = <85>;
				freq_max_cluster0 = <800000>;
				freq_max_cluster1 = <720000>;
			};
			throttle_tab_3 {
				temp = <90>;
				freq_max_cluster0 = <700000>;
				freq_max_cluster1 = <630000>;
			};
			throttle_tab_4 {
				temp = <95>;
				freq_max_cluster0 = <600000>;
				freq_max_cluster1 = <540000>;
			};
		};
	};

	cpu-id@14200290{
		compatible = "telechips,tcc-cpu-id";
		reg = <0x14200290 0x4 0x14200298 0x4 0x1420029C 0x4 0x14400014 0x4>;
		status = "okay";
	};

	run-control {
		compatible = "gpio-leds";
		green {
			gpios = <&gpd 1 0>;
			default-state = "on";
		};
		red {
			gpios = <&gpd 0 1>;
			default-state = "on";
		};
	};

	tcc_nsk_sc: tcc_nsk_sc@16671000 {
		compatible = "telechips,tcc-nsk-sc";

		rst_gpios = <&gpb 31 0>;
		detect_gpios = <&gpb 28 0>;
		pwren_gpios = <&gpb 30 0>;
		vccsel_gpios = <&gpc 31 0>;

		clk_gpios = <&gpa 26 0>;
		data_c7_gpios = <&gpa 25 0>;
		data_c4_gpios = <&gpa 27 0>;
		data_c8_gpios = <&gpa 28 0>;

		tda8024_use = <1>;

		pinctrl-names = "default", "io_data_c4", "io_data_c8";
		pinctrl-0 = <&nsk_sc_ctrl &nsk_sc_io_clk &nsk_sc_io_data0 &nsk_sc_io_gpio1 &nsk_sc_io_gpio2>;
		pinctrl-1 = <&nsk_sc_ctrl &nsk_sc_io_clk &nsk_sc_io_gpio0 &nsk_sc_io_data1 &nsk_sc_io_gpio2>;
		pinctrl-2 = <&nsk_sc_ctrl &nsk_sc_io_clk &nsk_sc_io_gpio0 &nsk_sc_io_gpio1 &nsk_sc_io_data2>;

		status = "okay";
	};
};

&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart2_data>;
	status = "okay";
};

/* Bluetooth */
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_data &uart1_rtscts>;
	status = "okay";
};
